//到2016-08-08 12:12分的时间选择器转化YYYYMMDDhhmmss
function changeTime(time){
	var times = "";
	times = String(time);
	if(times == null || times == ""){
		return;
	}
	times = times.split("-")[0] + times.split("-")[1] + String(times.split("-")[2]).substr(0,2) + String(times.split("-")[2]).substr(3,2) + String(times.split("-")[2]).substr(6,2) + "00";
	return times;
}
