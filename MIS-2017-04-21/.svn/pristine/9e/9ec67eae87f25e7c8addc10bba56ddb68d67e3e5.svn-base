package fsk.entity.site;

import java.util.List;

import com.framework.system.common.entity.dataDic.DataDicEntity;
import com.framework.system.db.dao.annotation.ColumnDescription;
import com.framework.system.db.dao.annotation.RelationlDescription;
import com.framework.system.db.dao.annotation.TableDescription;

import fsk.entity.employee.EmployeeEntity;
import fsk.entity.olderSite.OlderSiteEntity;

/**   
 * @Title: Entity
 * @Description: 站点信息表
 * @author feng.gu
 * @date 2017-03-06 12:24:50
 * @version V1.0   
 *
 */
@TableDescription(name = "B_SITE")
public class SiteEntity implements java.io.Serializable {
    public final static String ID = "ID";
    public final static String MASTER_EMPLOYEE_ID = "MASTER_EMPLOYEE_ID";
    public final static String NAME = "NAME";
    public final static String MAIN_PHOTO = "MAIN_PHOTO";
    public final static String CONTACT_PERSON = "CONTACT_PERSON";
    public final static String CONTACT_TEL = "CONTACT_TEL";
    public final static String PROVINCE_ID = "PROVINCE_ID";
    public final static String CITY_ID = "CITY_ID";
    public final static String AREA_ID = "AREA_ID";
    public final static String STREET_ID = "STREET_ID";
    public final static String ADDRESS = "ADDRESS";
    public final static String TYPE = "TYPE";
    
    /**
	 * 主键
	 */
	@ColumnDescription(name = "ID", isPrimaryKey = true, isAutoIncrement = true)
	private Integer id;
    /**
	 * 站长员工ID
	 */
	@ColumnDescription(name = "MASTER_EMPLOYEE_ID")
	private Integer masterEmployeeId;
    /**
	 * 站点名称
	 */
	@ColumnDescription(name = "NAME")
	private String name;
    /**
	 * 主图地址
	 */
	@ColumnDescription(name = "MAIN_PHOTO")
	private String mainPhoto;
    /**
	 * 联系人
	 */
	@ColumnDescription(name = "CONTACT_PERSON")
	private String contactPerson;
    /**
	 * 联系电话
	 */
	@ColumnDescription(name = "CONTACT_TEL")
	private String contactTel;
    /**
	 * 省
	 */
	@ColumnDescription(name = "PROVINCE_ID")
	private Integer provinceId;
    /**
	 * 市
	 */
	@ColumnDescription(name = "CITY_ID")
	private Integer cityId;
    /**
	 * 区
	 */
	@ColumnDescription(name = "AREA_ID")
	private Integer areaId;
    /**
	 * 街道
	 */
	@ColumnDescription(name = "STREET_ID")
	private Integer streetId;
    /**
	 * 具体地址
	 */
	@ColumnDescription(name = "ADDRESS")
	private String address;
    /**
	 * 站点类别
	 */
	@ColumnDescription(name = "TYPE")
	private String type;
        
    public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}
    public Integer getMasterEmployeeId() {
		return masterEmployeeId;
	}

	public void setMasterEmployeeId(Integer masterEmployeeId) {
		this.masterEmployeeId = masterEmployeeId;
	}
    public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
    public String getMainPhoto() {
		return mainPhoto;
	}

	public void setMainPhoto(String mainPhoto) {
		this.mainPhoto = mainPhoto;
	}
    public String getContactPerson() {
		return contactPerson;
	}

	public void setContactPerson(String contactPerson) {
		this.contactPerson = contactPerson;
	}
    public String getContactTel() {
		return contactTel;
	}

	public void setContactTel(String contactTel) {
		this.contactTel = contactTel;
	}
    public Integer getProvinceId() {
		return provinceId;
	}

	public void setProvinceId(Integer provinceId) {
		this.provinceId = provinceId;
	}
    public Integer getCityId() {
		return cityId;
	}

	public void setCityId(Integer cityId) {
		this.cityId = cityId;
	}
    public Integer getAreaId() {
		return areaId;
	}

	public void setAreaId(Integer areaId) {
		this.areaId = areaId;
	}
    public Integer getStreetId() {
		return streetId;
	}

	public void setStreetId(Integer streetId) {
		this.streetId = streetId;
	}
    public String getAddress() {
		return address;
	}

	public void setAddress(String address) {
		this.address = address;
	}
    public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}
        
    /**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="EmployeeEntity"	,joinColumn="ID"	)
			
			
		private EmployeeEntity masterEmployee;
	        
	public EmployeeEntity getMasterEmployee() {
		return masterEmployee;
    }
	
	public void setMasterEmployee(EmployeeEntity masterEmployee) {
		this.masterEmployee = masterEmployee;
	}
		/**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="DataDicEntity"	,joinColumn="ID"	)
			
			
		private DataDicEntity province;
	        
	public DataDicEntity getProvince() {
		return province;
    }
	
	public void setProvince(DataDicEntity province) {
		this.province = province;
	}
		/**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="DataDicEntity"	,joinColumn="ID"	)
			
			
		private DataDicEntity city;
	        
	public DataDicEntity getCity() {
		return city;
    }
	
	public void setCity(DataDicEntity city) {
		this.city = city;
	}
		/**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="DataDicEntity"	,joinColumn="ID"	)
			
			
		private DataDicEntity area;
	        
	public DataDicEntity getArea() {
		return area;
    }
	
	public void setArea(DataDicEntity area) {
		this.area = area;
	}
		/**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="DataDicEntity"	,joinColumn="ID"	)
			
			
		private DataDicEntity street;
	        
	public DataDicEntity getStreet() {
		return street;
    }
	
	public void setStreet(DataDicEntity street) {
		this.street = street;
	}
		/**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToMany",
			joinEntity="SiteEmployeeEntity"
					,joinColumn="SITE_ID"
			,inverseJoinColumn="EMPLOYEE_ID",inverseJoinEntity="EmployeeEntity")
	    private List<EmployeeEntity> employeeList;
	        
	public List<EmployeeEntity> getEmployeeList() {
		return employeeList;
    }
	
	public void setEmployeeList(List<EmployeeEntity> employeeList) {
		this.employeeList = employeeList;
	}
    		
			
		/**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToMany",
			joinEntity="SiteDataDicEntity"
					,joinColumn="SITE_ID"
			,inverseJoinColumn="DATA_DIC_ID",inverseJoinEntity="DataDicEntity")
	    private List<DataDicEntity> dataDicList;
	        
	public List<DataDicEntity> getDataDicList() {
		return dataDicList;
    }
	
	public void setDataDicList(List<DataDicEntity> dataDicList) {
		this.dataDicList = dataDicList;
	}
    		
			
		/**
     * 关系描述
	 */
	    @RelationlDescription(relation = "OneToMany",
		joinEntity="OlderSiteEntity"			,joinColumn="SITE_ID"
			)
			
		private List<OlderSiteEntity> OlderSiteList;
	        
	public List<OlderSiteEntity> getOlderSiteList() {
		return OlderSiteList;
	}
	
	public void setOlderSiteList(List<OlderSiteEntity> OlderSiteList) {
		this.OlderSiteList = OlderSiteList;
	}
			
			
}
