	/**
	 * 删除记录
	 * 
	 * @param id
	 *            主键
	 * @param obj
	 */
	public boolean del(Integer id, Boolean delCreateUser, Boolean delUser,
			Boolean delRole) {
		boolean result = false;
		if (id != null && id > 0) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				// 删除关联信息
				if (delCreateUser != null && delCreateUser.booleanValue()) {
					UserRoleEntity userRole = (UserRoleEntity) dbManager
							.getById(id, UserRoleEntity.class);
					if (userRole != null && userRole.getCreateUserId() != null) {
						dbManager.delNoTransaction(userRole.getCreateUserId(),
								UserEntity.class);
					}
				}
				// 删除关联信息
				if (delUser != null && delUser.booleanValue()) {
					UserRoleEntity userRole = (UserRoleEntity) dbManager
							.getById(id, UserRoleEntity.class);
					if (userRole != null && userRole.getUserId() != null) {
						dbManager.delNoTransaction(userRole.getUserId(),
								UserEntity.class);
					}
				}
				// 删除关联信息
				if (delRole != null && delRole.booleanValue()) {
					UserRoleEntity userRole = (UserRoleEntity) dbManager
							.getById(id, UserRoleEntity.class);
					if (userRole != null && userRole.getRoleId() != null) {
						dbManager.delNoTransaction(userRole.getRoleId(),
								RoleEntity.class);
					}
				}
				result = dbManager.delNoTransaction(id, UserRoleEntity.class);
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}

