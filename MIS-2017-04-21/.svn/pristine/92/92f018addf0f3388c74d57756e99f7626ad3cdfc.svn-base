	/**
	 * 保存记录
	 * 
	 * @param obj
	 */
	public boolean save(UserRoleEntity userRole) {
		boolean result = false;
		if (userRole != null) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				// 关联信息保存
				UserEntity createUser = userRole.getCreateUser();
				if (createUser != null) {
					dbManager.saveNoTransaction(createUser);
					userRole.setCreateUserId(createUser.getId());
				}
				// 关联信息保存
				UserEntity user = userRole.getUser();
				if (user != null) {
					dbManager.saveNoTransaction(user);
					userRole.setUserId(user.getId());
				}
				// 关联信息保存
				RoleEntity role = userRole.getRole();
				if (role != null) {
					dbManager.saveNoTransaction(role);
					userRole.setRoleId(role.getId());
				}
				result = dbManager.saveNoTransaction(userRole);
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}

	/**
	 * 批量保存记录
	 * 
	 * @param list
	 */
	public boolean saveList(List<UserRoleEntity> userRoleList) {
		boolean result = false;
		if (userRoleList != null && userRoleList.size() > 0) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				for (UserRoleEntity userRole : userRoleList) {
					if (userRole != null) {
						// 关联信息保存
						UserEntity createUser = userRole.getCreateUser();
						if (createUser != null) {
							dbManager.saveNoTransaction(createUser);
							userRole.setCreateUserId(createUser.getId());
						}
						// 关联信息保存
						UserEntity user = userRole.getUser();
						if (user != null) {
							dbManager.saveNoTransaction(user);
							userRole.setUserId(user.getId());
						}
						// 关联信息保存
						RoleEntity role = userRole.getRole();
						if (role != null) {
							dbManager.saveNoTransaction(role);
							userRole.setRoleId(role.getId());
						}
						result = dbManager.saveNoTransaction(userRole);
					}
				}
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}

	/**
	 * 保存
	 * 
	 * @param list
	 */
	public boolean saveList(String userId, String roleId, String userIds,
			String roleIds) {
		boolean result = false;
		if (userId != null || roleId != null) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				if (userId != null && !"".equals(userId)) {
					QueryCondition qc = new QueryCondition(
							UserRoleEntity.USER_ID, QueryCondition.eq, userId);
					result = dbManager.delByConditionsNoTransaction(
							UserRoleEntity.class, qc);
					if (result) {
						if (roleIds != null && !"".equals(roleIds)) {
							String[] sage = roleIds.split(",");
							if (sage != null && sage.length > 0) {
								for (int i = 0; i < sage.length; i++) {
									UserRoleEntity entity = new UserRoleEntity();
									entity.setUserId(Integer.valueOf(userId));
									entity.setRoleId(Integer.valueOf(sage[i]));
									result = dbManager
											.saveNoTransaction(entity);
								}
							}
						}
					}
				}
				if (roleId != null && !"".equals(roleId)) {
					QueryCondition qc = new QueryCondition(
							UserRoleEntity.ROLE_ID, QueryCondition.eq, roleId);
					result = dbManager.delByConditionsNoTransaction(
							UserRoleEntity.class, qc);
					if (result) {
						if (userIds != null && !"".equals(userIds)) {
							String[] sage = userIds.split(",");
							if (sage != null && sage.length > 0) {
								for (int i = 0; i < sage.length; i++) {
									UserRoleEntity entity = new UserRoleEntity();
									entity.setUserId(Integer.valueOf(sage[i]));
									entity.setRoleId(Integer.valueOf(roleId));
									result = dbManager
											.saveNoTransaction(entity);
								}
							}
						}
					}
				}
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}
	
