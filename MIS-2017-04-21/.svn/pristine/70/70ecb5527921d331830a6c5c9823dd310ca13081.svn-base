	 //运动功能评定MAS
	 function changeMAS(score){
	 	var name = "";
     	if(parseFloat(score) < 17){
     		name = "重度运动障碍";
     	}
     	if(parseFloat(score) > 16 && parseFloat(score) < 33){
     		name = "中度运动障碍";
     	}
     	if(parseFloat(score) > 32  && parseFloat(score) < 48){
     		name = "轻度运动障碍";
     	}
     	if(parseFloat(score) == 48){
     		name = "正常";
     	}
		return name;
	 }
	//日常生活评定改良Barthel说明
	 function changercsh_Barthel(score){
     	var name = "";
     	if(parseFloat(score) < 21){
     		name = "极严重功能障碍";
     	}
     	if(parseFloat(score) > 20 && parseFloat(score) < 46){
     		name = "严重功能障碍";
     	}
     	if(parseFloat(score) > 45 && parseFloat(score) < 71){
     		name = "中度功能缺陷";
     	}
     	if(parseFloat(score) > 45 && parseFloat(score) < 71){
     		name = "中度功能缺陷";
     	}
     	if(parseFloat(score) > 70 && parseFloat(score) < 100){
     		name = "轻度功能缺陷";
     	}
     	if(parseFloat(score) == 100){
     		name = "ADL自理";
     	}
		return name;
     }
	
	
