	 //FAC说明
	 function changeFAC(score){
     	var name = "";
     	if(parseFloat(score) <= 18){
     		name = "完全功能障碍";
     	}
     	if(parseFloat(score) > 18 && parseFloat(score) <= 35){
     		name = "极重度功能障碍";
     	}
     	if(parseFloat(score) > 35 && parseFloat(score) <= 53){
     		name = "重度功能障碍";
     	}
     	if(parseFloat(score) > 53 && parseFloat(score) <= 71){
     		name = "中度功能障碍";
     	}
     	if(parseFloat(score) > 71 && parseFloat(score) <= 89){
     		name = "轻度功能障碍";
     	}
     	if(parseFloat(score) > 89 && parseFloat(score) <= 107){
     		name = "综合功能基本正常";
     	}
     	if(parseFloat(score) > 107){
     		name = "综合功能正常";
     	}
		return name;
     }
     
