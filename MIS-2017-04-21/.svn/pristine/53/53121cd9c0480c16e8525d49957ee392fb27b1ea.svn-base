	   /**
		 * 保存记录
		 * 
		 * @param obj
		 */
		public boolean save(EdemaConditionEntity edemaCondition) {			
			boolean result =false;
			if(edemaCondition!=null){
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					 tx.beginTransaction();					 					 
					 					 				     				     					     //关联信息保存
					     ServiceTaskEntity serviceTask = edemaCondition.getServiceTask();
						 if(serviceTask!=null){						   
							 dbManager.saveNoTransaction(serviceTask);
							 edemaCondition.setServiceTaskId(serviceTask.getId());
						 }						 
				     				     				     					 					 result=dbManager.saveNoTransaction(edemaCondition);	
					 tx.commitAndClose();  
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {				    
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  			
			}	
			return result;
		}
		
		/**
		 * 批量保存记录
		 * 
		 * @param list
		 */
		public boolean saveList(List<EdemaConditionEntity> edemaConditionList) {
			boolean result = false;
			if (edemaConditionList != null && edemaConditionList.size() > 0) {
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					tx.beginTransaction(); 
					for(EdemaConditionEntity edemaCondition:edemaConditionList){
						if(edemaCondition!=null){												  								 
								 									 					                 								     								         //关联信息保存
									     ServiceTaskEntity serviceTask = edemaCondition.getServiceTask();
										 if(serviceTask!=null){						   
											 dbManager.saveNoTransaction(serviceTask);
											 edemaCondition.setServiceTaskId(serviceTask.getId());
										 }									 
								     								     								     								 								 result=dbManager.saveNoTransaction(edemaCondition);								 			
						}	
					}
				    tx.commitAndClose(); 
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  
			}
			return result;
		}
		
