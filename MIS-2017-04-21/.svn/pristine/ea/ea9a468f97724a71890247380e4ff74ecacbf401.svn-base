	   /**
		 * 保存记录
		 * 
		 * @param obj
		 */
		public boolean save(ServiceChildEntity serviceChild) {			
			boolean result =false;
			if(serviceChild!=null){
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					 tx.beginTransaction();					 					 
					 					 				     				     					     //关联信息保存
					     ServiceEntity service = serviceChild.getService();
						 if(service!=null){						   
							 dbManager.saveNoTransaction(service);
							 serviceChild.setServiceId(service.getId());
						 }						 
				     				     				     					 					 result=dbManager.saveNoTransaction(serviceChild);	
					 tx.commitAndClose();  
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {				    
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  			
			}	
			return result;
		}
		
		/**
		 * 批量保存记录
		 * 
		 * @param list
		 */
		public boolean saveList(List<ServiceChildEntity> serviceChildList) {
			boolean result = false;
			if (serviceChildList != null && serviceChildList.size() > 0) {
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					tx.beginTransaction(); 
					for(ServiceChildEntity serviceChild:serviceChildList){
						if(serviceChild!=null){												  								 
								 									 					                 								     								         //关联信息保存
									     ServiceEntity service = serviceChild.getService();
										 if(service!=null){						   
											 dbManager.saveNoTransaction(service);
											 serviceChild.setServiceId(service.getId());
										 }									 
								     								     								     								 								 result=dbManager.saveNoTransaction(serviceChild);								 			
						}	
					}
				    tx.commitAndClose(); 
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  
			}
			return result;
		}
		
