package fsk.entity.medicalHistory;

import java.math.BigDecimal;
import java.util.Date;
import java.util.List;

import fsk.entity.caseRecord.CaseRecordEntity;


import com.framework.system.db.dao.annotation.ColumnDescription;
import com.framework.system.db.dao.annotation.RelationlDescription;
import com.framework.system.db.dao.annotation.TableDescription;

/**   
 * @Title: Entity
 * @Description: 既往病史
 * @author feng.gu
 * @date 2016-07-19 11:09:36
 * @version V1.0   
 *
 */
@TableDescription(name = "B_MEDICAL_HISTORY")
public class MedicalHistoryEntity implements java.io.Serializable {
    public final static String ID = "ID";
    public final static String CASE_RECORD_ID = "CASE_RECORD_ID";
    public final static String DISEASE = "DISEASE";
    public final static String DISEASE_COURSE = "DISEASE_COURSE";
    public final static String MAIN_TREATMENT = "MAIN_TREATMENT";
    public final static String DISEASE_CONTROL = "DISEASE_CONTROL";
    public final static String OTHER = "OTHER";
    
    /**
	 * 主键
	 */
	@ColumnDescription(name = "ID", isPrimaryKey = true, isAutoIncrement = true)
	private Integer id;
    /**
	 * 病例ID
	 */
	@ColumnDescription(name = "CASE_RECORD_ID")
	private Integer caseRecordId;
    /**
	 * 疾病
	 */
	@ColumnDescription(name = "DISEASE")
	private String disease;
    /**
	 * 病程
	 */
	@ColumnDescription(name = "DISEASE_COURSE")
	private String diseaseCourse;
    /**
	 * 目前主要治疗
	 */
	@ColumnDescription(name = "MAIN_TREATMENT")
	private String mainTreatment;
    /**
	 * 疾病控制情况
	 */
	@ColumnDescription(name = "DISEASE_CONTROL")
	private String diseaseControl;
    /**
	 * 其他
	 */
	@ColumnDescription(name = "OTHER")
	private String other;
        
    public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}
    public Integer getCaseRecordId() {
		return caseRecordId;
	}

	public void setCaseRecordId(Integer caseRecordId) {
		this.caseRecordId = caseRecordId;
	}
    public String getDisease() {
		return disease;
	}

	public void setDisease(String disease) {
		this.disease = disease;
	}
    public String getDiseaseCourse() {
		return diseaseCourse;
	}

	public void setDiseaseCourse(String diseaseCourse) {
		this.diseaseCourse = diseaseCourse;
	}
    public String getMainTreatment() {
		return mainTreatment;
	}

	public void setMainTreatment(String mainTreatment) {
		this.mainTreatment = mainTreatment;
	}
    public String getDiseaseControl() {
		return diseaseControl;
	}

	public void setDiseaseControl(String diseaseControl) {
		this.diseaseControl = diseaseControl;
	}
    public String getOther() {
		return other;
	}

	public void setOther(String other) {
		this.other = other;
	}
        
    /**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="CaseRecordEntity"	,joinColumn="ID"	)
			
			
		private CaseRecordEntity caseRecord;
	        
	public CaseRecordEntity getCaseRecord() {
		return caseRecord;
    }
	
	public void setCaseRecord(CaseRecordEntity caseRecord) {
		this.caseRecord = caseRecord;
	}
			
}
