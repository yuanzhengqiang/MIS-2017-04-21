package weixin.controller;

import javax.servlet.http.HttpServletRequest;

import org.apache.log4j.Logger;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.servlet.ModelAndView;

@RequestMapping("/wechatServiceHistory")
@Controller
public class WeixinServiceHistoryController {
	private static Logger logger = Logger
			.getLogger(WeixinServiceHistoryController.class);

	/**
	 * 列表页
	 * 
	 * @param request
	 * @return
	 */
	@RequestMapping(params = "main")
	public ModelAndView center(HttpServletRequest request) {
		ModelAndView mav = new ModelAndView("/weixin/weixinServiceHistory/weixinServiceHistory");
		String olderId = request.getParameter("olderId");
		mav.addObject("olderId", olderId);
		return mav;
	}
	
	/**
	 * 详情页
	 * 
	 * @param request
	 * @return
	 */
	@RequestMapping(params = "mainXQ")
	public ModelAndView mainXQ(HttpServletRequest request) {
