	//自动生成
	function zdscjkbg(){
	   var bignqingzy_input = $("#bignqingzy_input").val();
	   var muqianwenti_input = $("#muqianwenti_input").val();
	   var jinqimb_input = $("#jinqimb_input").val();
	   var changqimb_input = $("#changqimb_input").val();
	   var xunlianjihua_input = $("#xunlianjihua_input").val();
	   var shuohua = "";
	   if((bignqingzy_input != null && bignqingzy_input != "") || (muqianwenti_input != null && muqianwenti_input != "") || (jinqimb_input != null && jinqimb_input != "") || (changqimb_input != null && changqimb_input != "") || (xunlianjihua_input != null && xunlianjihua_input != "")){
	   		shuohua = "您已经填入健康报告内容，需要覆盖吗?";
	   }
	   if(shuohua != ""){
	   	  if (!confirm(shuohua))  {  
	   	      return;
	   	  }
	   }
	   
	   var adlfenshu = $("#ADL_fenshuSum").html();
	   var adlshuoming = changeADL(adlfenshu);
	   var yyfxfenshu = $("#YYFX_fenshuSum").html();
	   var yyfxshuoming = changeYYFX(yyfxfenshu);
	   var ddfxfenshu = $("#DDFX_fenshuSum").html();
	   var ddfxshuoming = changeDDFX(ddfxfenshu);
	   var ycpdfenshu = $("#YCFX_input").val();
	   var ycpdshuoming = changeYCFX(ycpdfenshu);
	   var mmsefenshu = $("#MMSE_fenshuSum").html();
	   var mmseshuoming = changeMMSE(mmsefenshu);
			
	   var reqmsg="{'action':'CREATE_REPORT_BY_SCORE_REQUEST','content':{";
	   
	   if(adlfenshu != "" && adlfenshu != null){
	   	   reqmsg += "\"adl\":\"" + adlfenshu + "\",";
	   }
	   if(adlshuoming != "" && adlshuoming != null){
	   	   reqmsg += "\"adlDes\":\"" + adlshuoming + "\",";
	   }
	   if(yyfxfenshu != "" && yyfxfenshu != null){
	   	   reqmsg += "\"nutrition\":\"" + yyfxfenshu + "\",";
	   }
	   if(yyfxshuoming != "" && yyfxshuoming != null){
	   	   reqmsg += "\"nutritionDes\":\"" + yyfxshuoming + "\",";
	   }
	   if(ddfxfenshu != "" && ddfxfenshu != null){
	   	   reqmsg += "\"fall\":\"" + ddfxfenshu + "\",";
	   }
	   if(ddfxshuoming != "" && ddfxshuoming != null){
	   	   reqmsg += "\"fallDes\":\"" + ddfxshuoming + "\",";
	   }
	   if(ycpdfenshu != "" && ycpdfenshu != null){
	   	   reqmsg += "\"sore\":\"" + ycpdfenshu + "\",";
	   }
	   if(ycpdshuoming != "" && ycpdshuoming != null){
	   	   reqmsg += "\"soreDes\":\"" + ycpdshuoming + "\",";
	   }
	   if(mmsefenshu != "" && mmsefenshu != null){
	   	   reqmsg += "\"mmse\":\"" + mmsefenshu + "\",";
	   }
	   if(mmseshuoming != "" && mmseshuoming != null){
	   	   reqmsg += "\"mmseDes\":\"" + mmseshuoming + "\",";
	   }
		  
	   reqmsg += "}}";
	   
	   jQuery.ajax({
          type : "post",
          async:true,
          url : "healthReport.do?handler",
          dataType : "json",
          data: {
               "reqmsg":reqmsg
          },
          success : function(data){
              if(data.des=="success"){
            	  if (data.content != null) {
            	  	  $("#bignqingzy_input").val(data.content.diseaseSummary);
            	  	  $("#muqianwenti_input").val(data.content.mainProblem);
            	  	  $("#jinqimb_input").val(data.content.recentRehabilitationGoals);
            	  	  $("#changqimb_input").val(data.content.rehabilitationLongTermGoal);
            	  	  $("#xunlianjihua_input").val(data.content.trainingProgram);
            	  	  alert("生成成功");
            	  }
              }else if(data.des=="failure"){
                 alert("生成失败");
              }
          },
          error:function(){
	           alert("error");
          }
       });	
	}
	
