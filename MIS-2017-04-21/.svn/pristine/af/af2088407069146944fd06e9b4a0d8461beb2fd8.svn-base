	//根据出生年月日计算年龄yyyymmdd
	function getAge(strBirthday){
		var birthYear = strBirthday.substr(0,4);
		var d = new Date();
		var nowYear = d.getFullYear();
		var age = nowYear-birthYear;
		return age;
	}
