	//ADL说明
	 function changeADL(score){
     	var name = "";
     	if(parseFloat(score) <= 20){
     		name = "完全需要帮助";
     	}
     	if(parseFloat(score) > 20 && parseFloat(score) <= 40){
     		name = "需要很大帮助才能完成日常活动";
     	}
     	if(parseFloat(score) > 40 && parseFloat(score) < 60){
     		name = "需要帮助才能完成日常活动";
     	}
     	if(parseFloat(score) >= 60){
     		name = "基本能完成日常活动";
     	}
		return name;
     }
	//营养风险说明
	 function changeYYFX(score){
     	var name = "";
     	if(parseFloat(score) < 8){
     		name = "营养不良";
     	}
     	if(parseFloat(score) > 7 && parseFloat(score) < 12){
     		name = "营养不良风险";
     	}
     	if(parseFloat(score) > 11 && parseFloat(score) < 15){
     		name = "正常营养状态";
     	}
		return name;
     }
     //跌倒风险说明
	 function changeDDFX(score){
     	var name = "";
     	if(parseFloat(score) < 21){
     		name = "平衡能力差，只能坐轮椅，预示有跌倒的危险。";
     	}
     	if(parseFloat(score) > 20 && parseFloat(score) < 41){
     		name = "平衡能力可，能辅助步行，预示有跌倒的危险。";
     	}
     	if(parseFloat(score) > 40 && parseFloat(score) < 57){
     		name = "平衡能力好，能独立行走。";
     	}
		return name;
     }
     //压疮评定说明
     function changeYCFX(score){
		var name = "";
		if(score == "0期"){
			name="可疑的深部组织损伤 SubspectedDeep Tissue Injury";
		}
		if(score == "I期"){
			name="病变局限于表皮和真皮层（压红） Stage Ⅰ";
		}
  		if(score == "II期"){
			name="损伤超出真皮层（水泡） Stage Ⅱ";
		}	
  		if(score == "III期"){
			name="损伤深度达皮下脂肪组织（浅表破溃） Stage Ⅲ";
		}	
  		if(score == "IV期"){
			name="损伤深达肌层或者骨骼（深部损伤）Stage Ⅳ";
		}	
  		if(score == "V期"){
			name="不明确分期 Unstageable";
		}
		return name;
	 }
	 //MMSE说明
	 function changeMMSE(score){
	 	var name = "";
     	if(parseFloat(score) < 10){
     		name = "重度痴呆";
     	}
     	if(parseFloat(score) > 9 && parseFloat(score) < 21){
     		name = "中度痴呆";
     	}
     	if(parseFloat(score) > 20 && parseFloat(score) < 27){
     		name = "轻度痴呆";
     	}
     	if(parseFloat(score) > 26 && parseFloat(score) < 31){
     		name = "精神正常";
     	}
		return name;
	 }
	 
	
