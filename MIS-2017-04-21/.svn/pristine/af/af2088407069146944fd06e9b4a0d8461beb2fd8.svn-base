	//勾选单选框
	function getradiobox(value_,name){
 		var boxes = document.getElementsByName(name);
		for(i=0;i<boxes.length;i++){
			if(boxes[i].value == value_){
				boxes[i].checked = true;
				break;
			}
		}
	}
	
