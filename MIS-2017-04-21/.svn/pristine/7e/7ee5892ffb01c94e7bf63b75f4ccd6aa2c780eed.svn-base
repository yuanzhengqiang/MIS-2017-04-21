    }
	
	public void setServiceTask(ServiceTaskEntity serviceTask) {
		this.serviceTask = serviceTask;
	}
		/**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="EmployeeEntity"	,joinColumn="ID"	)
			
			
