	   /**
		 * 保存记录
		 * 
		 * @param obj
		 */
		public boolean save(DailyLifeAskEntity dailyLifeAsk) {			
			boolean result =false;
			if(dailyLifeAsk!=null){
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					 tx.beginTransaction();					 					 
					 					 				     				     					     //关联信息保存
					     ServiceTaskEntity serviceTask = dailyLifeAsk.getServiceTask();
						 if(serviceTask!=null){						   
							 dbManager.saveNoTransaction(serviceTask);
							 dailyLifeAsk.setServiceTaskId(serviceTask.getId());
						 }						 
				     				     				     					 					 result=dbManager.saveNoTransaction(dailyLifeAsk);	
					 tx.commitAndClose();  
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {				    
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  			
			}	
			return result;
		}
		
		/**
		 * 批量保存记录
		 * 
		 * @param list
		 */
		public boolean saveList(List<DailyLifeAskEntity> dailyLifeAskList) {
			boolean result = false;
			if (dailyLifeAskList != null && dailyLifeAskList.size() > 0) {
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					tx.beginTransaction(); 
					for(DailyLifeAskEntity dailyLifeAsk:dailyLifeAskList){
						if(dailyLifeAsk!=null){												  								 
								 									 					                 								     								         //关联信息保存
									     ServiceTaskEntity serviceTask = dailyLifeAsk.getServiceTask();
										 if(serviceTask!=null){						   
											 dbManager.saveNoTransaction(serviceTask);
											 dailyLifeAsk.setServiceTaskId(serviceTask.getId());
										 }									 
								     								     								     								 								 result=dbManager.saveNoTransaction(dailyLifeAsk);								 			
						}	
					}
				    tx.commitAndClose(); 
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  
			}
			return result;
		}
		
