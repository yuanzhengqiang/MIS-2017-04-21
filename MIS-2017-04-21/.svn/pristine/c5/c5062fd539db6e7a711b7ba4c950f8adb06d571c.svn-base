package weixin.controller;

import javax.servlet.http.HttpServletRequest;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.servlet.ModelAndView;

@RequestMapping("/wechatweimob")
@Controller
public class weimobController {
	
	/**
	 * 企业官网
	 * 
	 * @param request
	 * @return
	 */
	@RequestMapping(params = "corporateOfficialWebsite")
	public ModelAndView corporateOfficialWebsite(HttpServletRequest request) {
		ModelAndView mav = new ModelAndView("/weixin/wechatWeimob/corporateOfficialWebsite");
		return mav;
	}
	
	/**
	 * 老年商城
	 * 
	 * @param request
	 * @return
	 */
	@RequestMapping(params = "oldMall")
	public ModelAndView oldMall(HttpServletRequest request) {
		ModelAndView mav = new ModelAndView("/weixin/wechatWeimob/oldMall");
		return mav;
	}
	
	/**
	 * 历史信息
	 * 
	 * @param request
	 * @return
	 */
	@RequestMapping(params = "historicalInformation")
	public ModelAndView historicalInformation(HttpServletRequest request) {
		ModelAndView mav = new ModelAndView("/weixin/wechatWeimob/historicalInformation");
		return mav;
	}
	
	/**
	 * 长护险代理
	 * 
	 * @param request
	 * @return
	 */
	@RequestMapping(params = "longActingInsuranceAgent")
	public ModelAndView longActingInsuranceAgent(HttpServletRequest request) {
		ModelAndView mav = new ModelAndView("/weixin/wechatWeimob/longActingInsuranceAgent");
		return mav;
	}
	
	/**
	 * 在线客服
	 * 
	 * @param request
	 * @return
	 */
	@RequestMapping(params = "onlineService")
	public ModelAndView onlineService(HttpServletRequest request) {
		ModelAndView mav = new ModelAndView("/weixin/wechatWeimob/onlineService");
		return mav;
	}
	
}
