							 $("#cyrinput_hide").val("");
						});
						$("#fwxminput_Id4_hide").val("");
						$("#fwxminput_healthInsurance_hide").val();
						$("#fuwuzixianginput_hide").val("");
						$("#yjfwsc_hide").val("");
					});
					}
					$("#olderId_hide").val("");
					$("#fwName_hide").val("");
					$("#fwTel_hide").val("");
					$("#fwAddress_hide").val("");
            	}
            }else{
