function changeState(state){
     	var name = "未知";
     	switch (state)
		{
			case "0":
  			name = "制定中";
  			break;
  			
			case "1":
  			name = "待审核";
  			break;

			case "2":
  			name = "有效";
  			break;
  			
  			case "3":
  			name = "无效";
  			break;
  			
  			default:
  			break;
		}
		return name;
}

function create_gd2(){
	var name = $("#tjr_input").val();
	if(name == "" || name == null){
		alert("当前非员工,无法操作");
		return;
	}
	var state = $("#hljh_state_hide").val();
    if(state != "2"){
		alert("该护理计划状态为" + changeState(state) + "，无法生成工单");
		return;
	}
	var html = "是否确认生成？";
	var syscgdrq_hide = $("#syscgdrq_hide").val();
	if(syscgdrq_hide != null && syscgdrq_hide != ""){
		var date = new Date();
    	var month = date.getMonth() + 1;
    	if (month >= 1 && month <= 9) {
        	month = "0" + month;
    	}
    	var nowtime = date.getFullYear() + month;
    	var timecha = parseInt(syscgdrq_hide) - parseInt(nowtime);
    	if(timecha == 0){
    		html = "已自动生成过当月工单，是否继续生成？";
    	}
    	if(timecha == 1){
    		html = "已自动生成过下个月工单，是否继续生成？";
    	}
	}
	if (confirm(html))  {
	  var id = $("#id_hide").val();
	  var flag = create_gd_juti(id);
	  if(flag == 0){
	  	 alert("生成失败");
	  	 return;
	  }
	  if(flag == 1){
	  	 alert("生成成功");
	  	 return;
	  }
	  if(flag == 2){
	  	 alert("护理计划中还有服务未选择负责人，无法生成");
	  	 return;
	  }
	}
}
