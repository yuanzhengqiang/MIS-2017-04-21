			$("#xueya_tongji_highValue").html("");
			$("#xueya_tongji_lowValue").html("");
		}
		xueya_zhexiantu();
		$("#datacontainer_xueya").html(htmlcode);
		if (data.page != null) {
			genaratePaginateHtml_xueya(data.page.pageno, data.page.pageCount);
		} else {
			genaratePaginateHtml_xueya(1, 1);
		}
