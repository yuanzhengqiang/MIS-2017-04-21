package fsk.entity.employeePositionalTitle;

import com.framework.system.common.entity.dataDic.DataDicEntity;
import com.framework.system.db.dao.annotation.ColumnDescription;
import com.framework.system.db.dao.annotation.RelationlDescription;
import com.framework.system.db.dao.annotation.TableDescription;

import fsk.entity.employee.EmployeeEntity;

/**   
 * @Title: Entity
 * @Description: 员工职称关联表
 * @author feng.gu
 * @date 2016-06-21 08:39:50
 * @version V1.0   
 *
 */
@TableDescription(name = "R_EMPLOYEE_POSITIONAL_TITLE")
public class EmployeePositionalTitleEntity implements java.io.Serializable {
    public final static String ID = "ID";
    public final static String EMPLOYEE_ID = "EMPLOYEE_ID";
    public final static String POSITIONAL_TITLE_ID = "POSITIONAL_TITLE_ID";
    
    /**
	 * 主键
	 */
	@ColumnDescription(name = "ID", isPrimaryKey = true, isAutoIncrement = true)
	private Integer id;
    /**
	 * employeeId
	 */
	@ColumnDescription(name = "EMPLOYEE_ID")
	private Integer employeeId;
    /**
	 * positionalTitleId
	 */
	@ColumnDescription(name = "POSITIONAL_TITLE_ID")
	private Integer positionalTitleId;
        
    public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}
    public Integer getEmployeeId() {
		return employeeId;
	}

	public void setEmployeeId(Integer employeeId) {
		this.employeeId = employeeId;
	}
    public Integer getPositionalTitleId() {
		return positionalTitleId;
	}

	public void setPositionalTitleId(Integer positionalTitleId) {
		this.positionalTitleId = positionalTitleId;
	}
        
    /**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="EmployeeEntity"	,joinColumn="ID"	)
			
			
		private EmployeeEntity employee;
	        
	public EmployeeEntity getEmployee() {
		return employee;
    }
	
	public void setEmployee(EmployeeEntity employee) {
		this.employee = employee;
	}
		/**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="DataDicEntity"	,joinColumn="ID"	)
			
			
		private DataDicEntity positionalTitle;
	        
	public DataDicEntity getPositionalTitle() {
		return positionalTitle;
    }
	
	public void setPositionalTitle(DataDicEntity positionalTitle) {
		this.positionalTitle = positionalTitle;
	}
			
}
