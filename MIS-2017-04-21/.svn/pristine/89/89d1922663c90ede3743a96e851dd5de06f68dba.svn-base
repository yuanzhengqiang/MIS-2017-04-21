	        alert("请输入整数余额");  
	        return;
	    }  
	    if(yue2 != "" && !reg.test(yue2)){  
	        alert("请输入整数余额");  
	        return;
	    } */
		if(yue1 != "" && yue2 != "" && parseFloat(yue1)>parseFloat(yue2)){
