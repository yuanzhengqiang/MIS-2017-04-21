			customerState =	$("#customerState").val();
		}
		var idNumber_Card = $("#idNumber_Card").val();
		var siteIdsQuery = $("#siteIdsQuery").val();
		var hulijibie = $("#hulijibie").val();
		
		var quyushen_input = $("#quyushen_input").val();
	 	var quyushi_input = $("#quyushi_input").val();
