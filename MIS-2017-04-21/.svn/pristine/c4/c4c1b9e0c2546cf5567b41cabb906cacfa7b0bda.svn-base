package fsk.entity.servicePhoto;

import java.math.BigDecimal;
import java.util.Date;
import java.util.List;

import fsk.entity.service.ServiceEntity;


import com.framework.system.db.dao.annotation.ColumnDescription;
import com.framework.system.db.dao.annotation.RelationlDescription;
import com.framework.system.db.dao.annotation.TableDescription;

/**   
 * @Title: Entity
 * @Description: 服务项目图片
 * @author feng.gu
 * @date 2016-07-25 14:30:39
 * @version V1.0   
 *
 */
@TableDescription(name = "B_SERVICE_PHOTO")
public class ServicePhotoEntity implements java.io.Serializable {
    public final static String ID = "ID";
    public final static String SERVICE_ID = "SERVICE_ID";
    public final static String PHOTO_URL = "PHOTO_URL";
    public final static String SHOW_INDEX = "SHOW_INDEX";
    
    /**
	 * 主键
	 */
	@ColumnDescription(name = "ID", isPrimaryKey = true, isAutoIncrement = true)
	private Integer id;
    /**
	 * 服务项目ID
	 */
	@ColumnDescription(name = "SERVICE_ID")
	private Integer serviceId;
    /**
	 * 图片URL
	 */
	@ColumnDescription(name = "PHOTO_URL")
	private String photoUrl;
    /**
	 * 显示顺序
	 */
	@ColumnDescription(name = "SHOW_INDEX")
	private Integer showIndex;
        
    public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}
    public Integer getServiceId() {
		return serviceId;
	}

	public void setServiceId(Integer serviceId) {
		this.serviceId = serviceId;
	}
    public String getPhotoUrl() {
		return photoUrl;
	}

	public void setPhotoUrl(String photoUrl) {
		this.photoUrl = photoUrl;
	}
    public Integer getShowIndex() {
		return showIndex;
	}

	public void setShowIndex(Integer showIndex) {
		this.showIndex = showIndex;
	}
        
    /**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="ServiceEntity"	,joinColumn="ID"	)
			
			
		private ServiceEntity service;
	        
	public ServiceEntity getService() {
		return service;
    }
	
	public void setService(ServiceEntity service) {
		this.service = service;
	}
			
}
