package fsk.entity.siteEmployee;

import java.math.BigDecimal;
import java.util.Date;
import java.util.List;
import fsk.entity.site.SiteEntity;
import fsk.entity.employee.EmployeeEntity;
import com.framework.system.db.dao.annotation.ColumnDescription;
import com.framework.system.db.dao.annotation.RelationlDescription;
import com.framework.system.db.dao.annotation.TableDescription;

/**   
 * @Title: Entity
 * @Description: 站点员工关联表
 * @author feng.gu
 * @date 2016-06-21 08:39:47
 * @version V1.0   
 *
 */
@TableDescription(name = "R_SITE_EMPLOYEE")
public class SiteEmployeeEntity implements java.io.Serializable {
    public final static String ID = "ID";
    public final static String EMPLOYEE_ID = "EMPLOYEE_ID";
    public final static String SITE_ID = "SITE_ID";
    
    /**
	 * 主键
	 */
	@ColumnDescription(name = "ID", isPrimaryKey = true, isAutoIncrement = true)
	private Integer id;
    /**
	 * employeeId
	 */
	@ColumnDescription(name = "EMPLOYEE_ID")
	private Integer employeeId;
    /**
	 * siteId
	 */
	@ColumnDescription(name = "SITE_ID")
	private Integer siteId;
        
    public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}
    public Integer getEmployeeId() {
		return employeeId;
	}

	public void setEmployeeId(Integer employeeId) {
		this.employeeId = employeeId;
	}
    public Integer getSiteId() {
		return siteId;
	}

	public void setSiteId(Integer siteId) {
		this.siteId = siteId;
	}
        
    /**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="SiteEntity"	,joinColumn="ID"	)
			
			
		private SiteEntity site;
	        
	public SiteEntity getSite() {
		return site;
    }
	
	public void setSite(SiteEntity site) {
		this.site = site;
	}
		/**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="EmployeeEntity"	,joinColumn="ID"	)
			
			
		private EmployeeEntity employee;
	        
	public EmployeeEntity getEmployee() {
		return employee;
    }
	
	public void setEmployee(EmployeeEntity employee) {
		this.employee = employee;
	}
			
}
