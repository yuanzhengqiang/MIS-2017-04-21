	@ColumnDescription(name = "ID", isPrimaryKey = true, isAutoIncrement = true)
	private Integer id;
    /**
	 * 健康评估报告ID
	 */
	@ColumnDescription(name = "HEALTH_REPORT_ID")
	private Integer healthReportId;
    /**
	 * 图片URL
	 */
