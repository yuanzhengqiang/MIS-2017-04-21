	private String photoUrl;
    /**
	 * 显示顺序
	 */
	@ColumnDescription(name = "SHOW_INDEX")
	private Integer showIndex;
    /**
	 * 标题
	 */
	@ColumnDescription(name = "TITLE")
	private String title;
    /**
	 * 备注
	 */
	@ColumnDescription(name = "DES")
	private String des;
        
    public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}
    public Integer getHealthReportId() {
		return healthReportId;
	}

	public void setHealthReportId(Integer healthReportId) {
		this.healthReportId = healthReportId;
	}
    public String getPhotoUrl() {
		return photoUrl;
	}

	public void setPhotoUrl(String photoUrl) {
		this.photoUrl = photoUrl;
	}
    public Integer getShowIndex() {
		return showIndex;
	}

	public void setShowIndex(Integer showIndex) {
		this.showIndex = showIndex;
	}
    public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}
    public String getDes() {
		return des;
	}

	public void setDes(String des) {
		this.des = des;
	}
        
    /**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="HealthReportEntity"	,joinColumn="ID"	)
			
			
		private HealthReportEntity healthReport;
	        
	public HealthReportEntity getHealthReport() {
