	/**
	 * 循环嵌套查询子节点
	 * @param moduleEntity
	 * @return
	 */
	public ModuleEntity queryChildList(ModuleEntity moduleEntity){
		if(moduleEntity.getType()==1||moduleEntity.getType()==2){
			List<Object> objList = dbManager.searchListByColumn(ModuleEntity.class, ModuleEntity.PARENT_ID,String.valueOf(moduleEntity.getId()),ModuleEntity.SHOW_INDEX,"asc");
			if (objList != null && objList.size() > 0) {
				List<ModuleEntity> childModuleList = new ArrayList<ModuleEntity>();
				for (Object o : objList) {
					ModuleEntity obj = (ModuleEntity)o;
					obj = this.queryChildList(obj);
					childModuleList.add(obj);
				}
				moduleEntity.setChildModuleList(childModuleList);
			}
		}		
		return moduleEntity;
	}

	/**
	 * 删除记录
	 * 
	 * @param id
	 *            主键
	 * @param obj
	 */
	public boolean del(Integer id, Boolean delParentModule,
			Boolean delChildModuleList, Boolean delCreateUser,
			Boolean delUpdateUser) {
		boolean result = false;
		if (id != null && id > 0) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				// 删除关联信息
				if (delParentModule != null && delParentModule.booleanValue()) {
					ModuleEntity module = (ModuleEntity) dbManager.getById(id,
							ModuleEntity.class);
					if (module.getParentId() != null) {
						dbManager.delNoTransaction(module.getParentId(),
								ModuleEntity.class);
					}
				}
				// 删除关联信息
				if (delChildModuleList != null
						&& delChildModuleList.booleanValue()) {
					QueryCondition qc = new QueryCondition(
							ModuleEntity.PARENT_ID, QueryCondition.eq, id);
					dbManager.delByConditionsNoTransaction(ModuleEntity.class,
							qc);
				}
				// 删除关联信息
				if (delCreateUser != null && delCreateUser.booleanValue()) {
					ModuleEntity module = (ModuleEntity) dbManager.getById(id,
							ModuleEntity.class);
					if (module != null && module.getCreateUserId() != null) {
						dbManager.delNoTransaction(module.getCreateUserId(),
								UserEntity.class);
					}
				}
				// 删除关联信息
				if (delUpdateUser != null && delUpdateUser.booleanValue()) {
					ModuleEntity module = (ModuleEntity) dbManager.getById(id,
							ModuleEntity.class);
					if (module != null && module.getUpdateUserId() != null) {
						dbManager.delNoTransaction(module.getUpdateUserId(),
								UserEntity.class);
					}
				}
				result = dbManager.delNoTransaction(id, ModuleEntity.class);
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}

