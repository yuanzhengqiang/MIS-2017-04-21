	/**
	 * 保存记录
	 * 
	 * @param obj
	 */
	public boolean save(ModuleEntity module) {
		boolean result = false;
		if (module != null) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				// 关联信息保存
				ModuleEntity parentModule = module.getParentModule();
				if (parentModule != null) {
					dbManager.saveNoTransaction(parentModule);
					module.setParentId(parentModule.getId());
				}
				// 关联信息保存
				if (module.getChildModuleList() != null
						&& module.getChildModuleList().size() > 0) {
					for (ModuleEntity moduleEntity : module
							.getChildModuleList()) {
						dbManager.saveNoTransaction(moduleEntity);
					}
				}
				// 关联信息保存
				UserEntity createUser = module.getCreateUser();
				if (createUser != null) {
					dbManager.saveNoTransaction(createUser);
					module.setCreateUserId(createUser.getId());
				}
				// 关联信息保存
				UserEntity updateUser = module.getUpdateUser();
				if (updateUser != null) {
					dbManager.saveNoTransaction(updateUser);
					module.setUpdateUserId(updateUser.getId());
				}
				result = dbManager.saveNoTransaction(module);
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}

	/**
	 * 批量保存记录
	 * 
	 * @param list
	 */
	public boolean saveList(List<ModuleEntity> moduleList) {
		boolean result = false;
		if (moduleList != null && moduleList.size() > 0) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				for (ModuleEntity module : moduleList) {
					if (module != null) {
						// 关联信息保存
						ModuleEntity parentModule = module.getParentModule();
						if (parentModule != null) {
							dbManager.saveNoTransaction(parentModule);
							module.setParentId(parentModule.getId());
						}
						// 关联信息保存
						if (module.getChildModuleList() != null
								&& module.getChildModuleList().size() > 0) {
							for (ModuleEntity moduleEntity : module
									.getChildModuleList()) {
								dbManager.saveNoTransaction(moduleEntity);
							}
						}
						// 关联信息保存
						UserEntity createUser = module.getCreateUser();
						if (createUser != null) {
							dbManager.saveNoTransaction(createUser);
							module.setCreateUserId(createUser.getId());
						}
						// 关联信息保存
						UserEntity updateUser = module.getUpdateUser();
						if (updateUser != null) {
							dbManager.saveNoTransaction(updateUser);
							module.setUpdateUserId(updateUser.getId());
						}
						result = dbManager.saveNoTransaction(module);
					}
				}
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}

