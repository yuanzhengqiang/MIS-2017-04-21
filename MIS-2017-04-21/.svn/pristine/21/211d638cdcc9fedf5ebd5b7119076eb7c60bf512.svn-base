	/**
	 * 根据id读取记录
	 * 
	 * @param id
	 *            主键
	 * @param parentModuleShow
	 *            是否查询关联信息
	 * @param childModuleListShow
	 *            是否查询关联信息
	 * @param createUserShow
	 *            是否查询关联信息
	 * @param updateUserShow
	 *            是否查询关联信息
	 * @param obj
	 */
	public ModuleEntity getById(Integer id, Boolean parentModuleShow,
			Boolean childModuleListShow, Boolean createUserShow,
			Boolean updateUserShow) {
		ModuleEntity obj = null;
		if (id != null) {
			obj = (ModuleEntity) dbManager.getById(id, ModuleEntity.class);
			// 查询关联内容
			if (parentModuleShow != null && parentModuleShow.booleanValue()
					&& obj != null && obj.getParentId() > 0) {
				ModuleEntity module = (ModuleEntity) dbManager.getById(
						obj.getParentId(), ModuleEntity.class);
				obj.setParentModule(module);
			}
			// 查询关联内容
			if (childModuleListShow != null
					&& childModuleListShow.booleanValue() && obj != null
					&& obj.getId() > 0) {
				List<Object> list = dbManager.searchListByColumn(
						ModuleEntity.class, ModuleEntity.PARENT_ID,
						String.valueOf(id));
				if (list != null && list.size() > 0) {
					List<ModuleEntity> childModuleList = new ArrayList<ModuleEntity>();
					for (Object o : list) {
						childModuleList.add((ModuleEntity) o);
					}
					obj.setChildModuleList(childModuleList);
				}
			}
			// 查询关联内容
			if (createUserShow != null && createUserShow.booleanValue()
					&& obj != null && obj.getCreateUserId() > 0) {
				UserEntity createUser = (UserEntity) dbManager.getById(
						obj.getCreateUserId(), UserEntity.class);
				obj.setCreateUser(createUser);
			}
			// 查询关联内容
			if (updateUserShow != null && updateUserShow.booleanValue()
					&& obj != null && obj.getUpdateUserId() > 0) {
				UserEntity updateUser = (UserEntity) dbManager.getById(
						obj.getUpdateUserId(), UserEntity.class);
				obj.setUpdateUser(updateUser);
			}
		}
		return obj;
	}

