package fsk.entity.healthReport;

import java.math.BigDecimal;
import java.util.Date;
import java.util.List;

import fsk.entity.employee.EmployeeEntity;
import fsk.entity.healthReportPhoto.HealthReportPhotoEntity;


import com.framework.system.db.dao.annotation.ColumnDescription;
import com.framework.system.db.dao.annotation.RelationlDescription;
import com.framework.system.db.dao.annotation.TableDescription;

/**   
 * @Title: Entity
 * @Description: 健康评估报告
 * @author feng.gu
 * @date 2016-07-12 15:06:40
 * @version V1.0   
 *
 */
@TableDescription(name = "B_HEALTH_REPORT")
public class HealthReportEntity implements java.io.Serializable {
    public final static String ID = "ID";
    public final static String ASSESSMENT_TIME = "ASSESSMENT_TIME";
    public final static String EMPLOYEE_ID = "EMPLOYEE_ID";
    public final static String DISEASE_SUMMARY = "DISEASE_SUMMARY";
    public final static String RECENT_REHABILITATION_GOALS = "RECENT_REHABILITATION_GOALS";
    public final static String REHABILITATION_LONG_TERM_GOAL = "REHABILITATION_LONG_TERM_GOAL";
    public final static String TRAINING_PROGRAM = "TRAINING_PROGRAM";
    public final static String STATUS = "STATUS";
    public final static String MAIN_PROBLEM = "MAIN_PROBLEM";
    public final static String SUGGEST = "SUGGEST";
    
    /**
	 * 主键
	 */
	@ColumnDescription(name = "ID", isPrimaryKey = true, isAutoIncrement = true)
	private Integer id;
    /**
	 * 健康报告评估时间
	 */
	@ColumnDescription(name = "ASSESSMENT_TIME")
	private String assessmentTime;
    /**
	 * 健康报告评估员工ID
	 */
	@ColumnDescription(name = "EMPLOYEE_ID")
	private Integer employeeId;
    /**
	 * 病情摘要
	 */
	@ColumnDescription(name = "DISEASE_SUMMARY")
	private String diseaseSummary;
    /**
	 * 康复近期目标
	 */
	@ColumnDescription(name = "RECENT_REHABILITATION_GOALS")
	private String recentRehabilitationGoals;
    /**
	 * 康复远期目标
	 */
	@ColumnDescription(name = "REHABILITATION_LONG_TERM_GOAL")
	private String rehabilitationLongTermGoal;
    /**
	 * 训练计划
	 */
	@ColumnDescription(name = "TRAINING_PROGRAM")
	private String trainingProgram;
    /**
	 * 健康报告评估状态（1-未完成 2-已完成）
	 */
	@ColumnDescription(name = "STATUS")
	private Integer status;
    /**
	 * 目前存在主要问题
	 */
	@ColumnDescription(name = "MAIN_PROBLEM")
	private String mainProblem;
    /**
	 * 辅具使用建议
	 */
	@ColumnDescription(name = "SUGGEST")
	private String suggest;
        
    public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}
    public String getAssessmentTime() {
		return assessmentTime;
	}

	public void setAssessmentTime(String assessmentTime) {
		this.assessmentTime = assessmentTime;
	}
    public Integer getEmployeeId() {
		return employeeId;
	}

	public void setEmployeeId(Integer employeeId) {
		this.employeeId = employeeId;
	}
    public String getDiseaseSummary() {
		return diseaseSummary;
	}

	public void setDiseaseSummary(String diseaseSummary) {
		this.diseaseSummary = diseaseSummary;
	}
    public String getRecentRehabilitationGoals() {
		return recentRehabilitationGoals;
	}

	public void setRecentRehabilitationGoals(String recentRehabilitationGoals) {
		this.recentRehabilitationGoals = recentRehabilitationGoals;
	}
    public String getRehabilitationLongTermGoal() {
		return rehabilitationLongTermGoal;
	}

	public void setRehabilitationLongTermGoal(String rehabilitationLongTermGoal) {
		this.rehabilitationLongTermGoal = rehabilitationLongTermGoal;
	}
    public String getTrainingProgram() {
		return trainingProgram;
	}

	public void setTrainingProgram(String trainingProgram) {
		this.trainingProgram = trainingProgram;
	}
    public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}
    public String getMainProblem() {
		return mainProblem;
	}

	public void setMainProblem(String mainProblem) {
		this.mainProblem = mainProblem;
	}
    public String getSuggest() {
		return suggest;
	}

	public void setSuggest(String suggest) {
		this.suggest = suggest;
	}
        
    /**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="EmployeeEntity"	,joinColumn="ID"	)
			
			
		private EmployeeEntity employee;
	        
	public EmployeeEntity getEmployee() {
		return employee;
    }
	
	public void setEmployee(EmployeeEntity employee) {
		this.employee = employee;
	}
		/**
     * 关系描述
	 */
	    @RelationlDescription(relation = "OneToMany",
		joinEntity="HealthReportPhotoEntity"			,joinColumn="HEALTH_REPORT_ID"
			)
			
		private List<HealthReportPhotoEntity> healthReportPhotoList;
	        
	public List<HealthReportPhotoEntity> getHealthReportPhotoList() {
		return healthReportPhotoList;
	}
	
	public void setHealthReportPhotoList(List<HealthReportPhotoEntity> healthReportPhotoList) {
		this.healthReportPhotoList = healthReportPhotoList;
	}
			
			
}
