	private Integer devId;
    /**
	 * 设备类型（1-老人定位器 2-安护宝）
	 */
	@ColumnDescription(name = "DEV_TYPE")
	private Integer devType;
    /**
	 * 老人姓名
	 */
	@ColumnDescription(name = "OLDER_NAME")
	private String olderName;
    /**
