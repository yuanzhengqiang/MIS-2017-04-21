	/**
	 * 保存记录
	 * 
	 * @param obj
	 */
	public boolean save(
			PhysiologicalParametersCurrentEntity physiologicalParametersCurrent) {
		boolean result = false;
		if (physiologicalParametersCurrent != null) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				// 关联信息保存
				OlderEntity older = physiologicalParametersCurrent.getOlder();
				if (older != null) {
					dbManager.saveNoTransaction(older);
					physiologicalParametersCurrent.setOlderId(older.getId());
				}
				result = dbManager
						.saveNoTransaction(physiologicalParametersCurrent);
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}

	/**
	 * 批量保存记录
	 * 
	 * @param list
	 */
	public boolean saveList(
			List<PhysiologicalParametersCurrentEntity> physiologicalParametersCurrentList) {
		boolean result = false;
		if (physiologicalParametersCurrentList != null
				&& physiologicalParametersCurrentList.size() > 0) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				for (PhysiologicalParametersCurrentEntity physiologicalParametersCurrent : physiologicalParametersCurrentList) {
					if (physiologicalParametersCurrent != null) {
						// 关联信息保存
						OlderEntity older = physiologicalParametersCurrent
								.getOlder();
						if (older != null) {
							dbManager.saveNoTransaction(older);
							physiologicalParametersCurrent.setOlderId(older
									.getId());
						}
						result = dbManager
								.saveNoTransaction(physiologicalParametersCurrent);
					}
				}
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}

