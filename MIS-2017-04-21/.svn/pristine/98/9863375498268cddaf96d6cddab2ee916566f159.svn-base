	<script type="text/javascript">
	//根据排序规则重新查询
	function queryBySort(par,column){
		var classFlag = listSortClickEvent(par);
		var type = judgmentChinese(column);
		if(type != null && type != ""){
			classFlag = type + classFlag;
			column = column.substr(8,column.length);
		}
		$("#sortType").val(classFlag);
		$("#sortColumn").val(column);
		go2page12(currentshownpage12);
	}

	//iframe内嵌页面设置高度
    function reset() {
  		window.parent.reset();
	}
		
		
    function doAdd() {
		document.getElementById("addA").click();
	}

    $(document).ready(function(){
    	var olderId="<%=request.getAttribute("olderId")%>";
    	$("#olderId_hide").val(olderId);
    	 
    	$("#addA").attr("href","<%=request.getContextPath()%>" + "/examinationRecord.do?mainXQ&olderId=" + olderId);
    	
        //initialize the javascript
        query12();
       
    	$(".deleteThisTime").click(function(){               //清除日期框中的值
    	       $(this).parent().prev()[0].value = "";
    	    });
        App.init();
		$('div[class="icheckbox_square-blue checkbox checked disabled"]').removeClass("disabled");//美化checkbox框
	});
	
	//日期转化2016-10-10 20161010
	function formatDateCGtoD2(time){
		if(time == null || time == "" || time.length != 10){
			time = "";
			return time;
		}
		time = time.substring(0, 4) + time.substring(5, 7) + time.substring(8, 10);
		return time; 
	}
	
	function del2(id) {
	  if (confirm("是否确认删除?"))  {  
		jQuery.ajax({
			type:"post",
			url:"examinationRecord.do?del",
			async:true,
			dataType:"json",
			data:{ids:id},
			success:function(data) {
				alert(data.des);
				if (data.result == "success") {
					go2page12(currentshownpage12);
				}else if(data.des=="failure"){
                    alert("删除失败");
                }
			},
			error:function() {
				alert("error");
			}
		});
	  }
	}
	
	//体检记录////////////////////////////////////////////////
	var currentshownpage12 = 1;
	function query12() {
		go2page12(1);
	}

	function go2page12(pagenumber){
		var pagesize = $("#pageSizeSelector12 option:selected").val();
		
		var tjjg_input = $("#tjjg_input").val();//体检机构
		var jlr_input = $("#jlr_input").val();//记录人
		var tjsjStart_input = $("#tjsjStart_input").val();//体检时间开始
		var tjsjEnd_input = $("#tjsjEnd_input").val();//体检时间结束
		var jlsjStart_input = $("#jlsjStart_input").val();//记录时间开始
		var jlsjEnd_input = $("#jlsjEnd_input").val();//记录时间结束
		
		var olderId = $("#olderId_hide").val();//老人id
	
		var reqmsg="{'action':'QUERY_EXAMINATION_RECORD_LIST_REQUEST',";
		
		var sortType = $("#sortType").val();
		var sortColumn = $("#sortColumn").val();
		if(sortType != null && sortType != ""){
			reqmsg += "'order':[{'column':'" + sortColumn + "','type':'" + sortType + "'}],";
		}
	
		reqmsg += "'page':{'pageno':'" + pagenumber + "','pagesize':'" + pagesize + "'},'content':{";
			
		if (olderId != null && olderId != "0") {
			reqmsg += "\"olderId\":" + olderId + ",";
		}
		if (tjjg_input != null && tjjg_input != "") {
			reqmsg += "\"examinationOrganization_like\":\"" + tjjg_input + "\",";
		}
		if (jlr_input != null && jlr_input != "") {
			reqmsg += "\"createEmployeeName\":\"" + jlr_input + "\",";
		}
		if (tjsjStart_input != null && tjsjStart_input != "") {
		    tjsjStart_input = formatDateCGtoD2(tjsjStart_input) + "000001";
			reqmsg += "\"examinationTime_ge\":\"" + tjsjStart_input + "\",";
		}
		if (tjsjEnd_input != null && tjsjEnd_input != "") {
			tjsjEnd_input = formatDateCGtoD2(tjsjEnd_input) + "235959";
			reqmsg += "\"examinationTime_le\":\"" + tjsjEnd_input + "\",";
		}
		if(tjsjStart_input != null && tjsjStart_input != "" && tjsjEnd_input != null && tjsjEnd_input != ""){
			if(tjsjStart_input > tjsjEnd_input){
				alert("体检开始时间不能大于结束时间");
				return;
			}
		}
		if (jlsjStart_input != null && jlsjStart_input != "") {
		    jlsjStart_input = formatDateCGtoD2(jlsjStart_input) + "000001";
			reqmsg += "\"createTime_ge\":\"" + jlsjStart_input + "\",";
		}
		if (jlsjEnd_input != null && jlsjEnd_input != "") {
			jlsjEnd_input = formatDateCGtoD2(jlsjEnd_input) + "235959";
			reqmsg += "\"createTime_le\":\"" + jlsjEnd_input + "\",";
		}
		if(jlsjStart_input != null && jlsjStart_input != "" && jlsjEnd_input != null && jlsjEnd_input != ""){
			if(jlsjStart_input > jlsjEnd_input){
				alert("记录开始时间不能大于结束时间");
				return;
			}
		}
		
		reqmsg += "}}";
	
	    jQuery.ajax({
	          type : "post",
	          async:true,
	          url : "examinationRecord.do?handler",
	          dataType : "json",
	          data: {
	               "reqmsg":reqmsg
	          },
	          success : function(data){
	              if(data.des=="success"){
	            	  changeData12(data);
	              }else if(data.des=="failure"){
	                 alert("查询失败");
	              }
	          },
	          error:function(){
		           alert("error");
	          }
	     });
	}

	function changeData12(data){
		var htmlcode = "";
		if (data.content != null) {
			jQuery.each(data.content.examinationRecordList, function(i, item) {
				htmlcode += "<tr>";		
			    htmlcode += "<td>" + formateTime(item.examinationTime) + "</td>";	
			    htmlcode += "<td>" + item.olderAge + "</td>";
			    htmlcode += "<td>" + item.examinationOrganization + "</td>";
			    htmlcode += "<td>" + item.contactTel + "</td>";
			    htmlcode += "<td>" + item.createEmployeeName + "</td>";
			    htmlcode += "<td>" + formateTime(item.createTime) + "</td>";
			    
				htmlcode += "<td><div class=\"btn-group\">";
				htmlcode += "<button class=\"btn btn-default btn-xs\" type=\"button\">操作</button>";
				htmlcode += "<button data-toggle=\"dropdown\" class=\"btn btn-xs btn-primary dropdown-toggle\" type=\"button\">";
				htmlcode += "<span class=\"caret\" style=\"padding:0;\"></span>";
				htmlcode += "<span class=\"sr-only\">Toggle Dropdown</span>";
				htmlcode += "</button>";
				htmlcode += "<ul role=\"menu\" class=\"dropdown-menu pull-right\">";
				var headUrl = "<%=request.getContextPath()%>";
				<c:if test="${older_querytjjlXQ_opt_control != 'yes' or older_querytjjlXQ_opt_show == 'yes'}">
					htmlcode += "<li><a href=\""+headUrl+"/examinationRecord.do?mainXQ&id=" + item.id + "\" target=\"_blank\">查看详情</a></li>";
				</c:if>
				<c:if test="${older_deltjjlXQ_opt_control != 'yes' or older_deltjjlXQ_opt_show == 'yes'}">
					htmlcode += "<li onclick=\"del2('" + item.id + "')\"><a href=\"###\">删除</a></li>";
				</c:if>
				htmlcode += "</ul>";
				htmlcode += "</div></td>";		
				htmlcode += "</tr>";
			});
		}
		$("#datacontainer11").html(htmlcode);
		if (data.page != null) {
			genaratePaginateHtml12(data.page.pageno, data.page.pageCount);
			genarateRecordNumberHtml12(data.page.pageno, data.page.pagesize, data.page.recordCount);
		} else {
			genaratePaginateHtml12(1, 1);
			genarateRecordNumberHtml12(1, 10, 0);
		}
		reset();
	}
	/**
	 * 生成记录数信息
	 * @method genarateRecordNumberHtml12
	 * @param {Number} currentpage 当前页
	 * @param {Number} pagesize 每页记录条目数
	 * @param {Number} totalRecord 总记录数
	 */
	function genarateRecordNumberHtml12(currentpage, pagesize, totalRecord) {
		var recordInfos = "";
		if (totalRecord > 0) {
		    recordInfos = "当前显示" + totalRecord + "条总记录";
			recordInfos += "中第" + ((currentpage - 1) * pagesize + 1) + "-";
			if (currentpage * pagesize > totalRecord) {
				recordInfos += totalRecord;
			} else {
				recordInfos += currentpage * pagesize;
			}
			recordInfos += "条 ";
		} else {
			recordInfos = "当前显示0 条总记录中的第0-0条";
		}
		
		$("#datatable_info12").html(recordInfos);
	}
	
	/**
	 * 生成分页信息
	 * @method genaratePaginateHtml12
	 * @param {Number} currentpage 当前页
	 * @param {Number} totalpage 总页数
	 */
	function genaratePaginateHtml12(currentpage, totalpage, pagesize) {
		currentshownpage12 = currentpage;
	
		var htmlcode = "";
		
		//上一页
		if (currentpage > 1) {
			htmlcode += "<li class=\"pre\"><a href=\"#\" onclick=\"go2page12('1')\"><span class=\"fa fa-angle-left\"></span>&nbsp;首页</a></li>";
			htmlcode += "<li class=\"pre\"><a href=\"#\" onclick=\"go2page12('" + (currentpage - 1) + "')\"><span class=\"fa fa-angle-left\"></span>&nbsp;上一页</a></li>";
		} else {
			htmlcode += "<li class=\"pre disabled\"><a href=\"#\"><span class=\"fa fa-angle-left\"></span>&nbsp;首页</a></li>";
			htmlcode += "<li class=\"pre disabled\"><a href=\"#\"><span class=\"fa fa-angle-left\"></span>&nbsp;上一页</a></li>";
		}
		
		//具体页码
		if (totalpage <= 5) {
			for (var i = 1; i <= totalpage; i++) {
				if (currentpage == i) {
					htmlcode += "<li class=\"active\"><a href=\"#\">" + i + "</a></li>";
				} else {
					htmlcode += "<li><a href=\"#\" onclick=\"go2page12('" + i + "')\">" + i + "</a></li>";
				}
			}
		} else {
			var startpage = currentpage;
			var endpage = currentpage;
			while (true) {
				if (endpage - startpage >= 4) {
					break;
				} else {
					if (startpage > 1) {
						startpage = startpage - 1;
					}
					if (endpage < totalpage) {
						endpage = endpage - (-1);
					}
				}
			}
			
			for (var i = startpage; i <= endpage; i++) {
				if (currentpage == i) {
					htmlcode += "<li class=\"active\"><a href=\"#\">" + i + "</a></li>";
				} else {
					htmlcode += "<li><a href=\"#\" onclick=\"go2page12('" + i + "')\">" + i + "</a></li>";
				}
			}
		}
		
		//下一页
		if (totalpage > currentpage) {
			htmlcode += "<li class=\"next\"><a href=\"#\" onclick=\"go2page12('" + (parseInt(currentpage) + 1) + "')\">下一页&nbsp;<span class=\"fa fa-angle-right\"></span></a></li>";
			htmlcode += "<li class=\"next\"><a href=\"#\" onclick=\"go2page12('" + parseInt(totalpage) + "')\">末页&nbsp;<span class=\"fa fa-angle-right\"></span></a></li>";
		} else {
			htmlcode += "<li class=\"next disabled\"><a href=\"#\">下一页&nbsp;<span class=\"fa fa-angle-right\"></span></a></li>";
			htmlcode += "<li class=\"next disabled\"><a href=\"#\">末页&nbsp;<span class=\"fa fa-angle-right\"></span></a></li>";
		}
		$("#paginationArea12").html(htmlcode);
	}
	</script>
	
