@TableDescription(name = "R_OLDER_WECHAT")
public class OlderWechatEntity implements java.io.Serializable {
    public final static String ID = "ID";
    public final static String OLDER_ID = "OLDER_ID";
    public final static String WECHAT_ID = "WECHAT_ID";
    public final static String ADD_TIME = "ADD_TIME";
    
    /**
	 * 主键
	 */
	@ColumnDescription(name = "ID", isPrimaryKey = true, isAutoIncrement = true)
	private Integer id;
    /**
	 * 老人会员主键
	 */
	@ColumnDescription(name = "OLDER_ID")
	private Integer olderId;
    /**
	 * 微信会员主键
	 */
	@ColumnDescription(name = "WECHAT_ID")
	private Integer wechatId;
    /**
	 * 绑定时间(YYYYMMDDhhmmss)
	 */
	@ColumnDescription(name = "ADD_TIME")
	private String addTime;
        
    public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}
    public Integer getOlderId() {
		return olderId;
	}

	public void setOlderId(Integer olderId) {
		this.olderId = olderId;
	}
    public Integer getWechatId() {
		return wechatId;
	}

	public void setWechatId(Integer wechatId) {
		this.wechatId = wechatId;
	}
    public String getAddTime() {
		return addTime;
	}

	public void setAddTime(String addTime) {
		this.addTime = addTime;
	}
        
    /**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="OlderEntity"	,joinColumn="ID"	)
			
			
		private OlderEntity older;
	        
	public OlderEntity getOlder() {
		return older;
    }
	
	public void setOlder(OlderEntity older) {
		this.older = older;
	}
		/**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="WechatEntity"	,joinColumn="ID"	)
			
			
		private WechatEntity wechat;
	        
	public WechatEntity getWechat() {
		return wechat;
    }
	
	public void setWechat(WechatEntity wechat) {
		this.wechat = wechat;
	}
			
}
