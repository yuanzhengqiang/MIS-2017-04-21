	function hulijibieChange(id){
		var name="未知";
     	switch (id)
		{
			case 1:
  			name="1级";
  			break;
  			
			case 2:
  			name="2级";
  			break;

			case 3:
  			name="3级";
  			break;
  			
  			case 4:
  			name="4级";
  			break;
  			
			case 5:
  			name="5级";
  			break;

			case 6:
  			name="6级";
  			break;
  			
  			default:
  			break;
		}
		return name;
	}
	//客户状态
	function khztChange(id){
		var name="未知";
     	switch (id)
		{
			case 0:
  			name="正常";
  			break;
  			
			case 1:
  			name="审核不通过";
  			break;

			case 2:
  			name="待评估";
  			break;
  			
  			default:
  			break;
		}
		return name;
	}
