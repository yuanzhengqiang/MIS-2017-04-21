		/**
		 * 删除记录
		 * 
		 * @param id 主键
		 * @param obj
		 */
		public boolean del(Integer id,Boolean delCaseRecord) {
			boolean result = false;
			if (id !=null&&id>0) {
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					tx.beginTransaction(); 					
										 	                 				     					    //删除关联信息
						if(delCaseRecord!=null&&delCaseRecord.booleanValue()){	
							MedicalHistoryEntity medicalHistory = (MedicalHistoryEntity)dbManager.getById(id, MedicalHistoryEntity.class);
							if(medicalHistory!=null&&medicalHistory.getCaseRecordId()!=null){
								dbManager.delNoTransaction(medicalHistory.getCaseRecordId(), CaseRecordEntity.class);
							}						
						}
				     				     				     										result = dbManager.delNoTransaction(id, MedicalHistoryEntity.class);
					tx.commitAndClose(); 
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  
			}
			return result;
		}
		
		/**
		 * 批量删除记录
		 * 
		 * @param ids
		 *            主键 英文逗号间隔
		 * @param obj
		 */
		public boolean del(String ids) {
			boolean result = false;
			if (ids != null && !"".equals(ids.trim())) {
				TransactionManager tx = DbUtils.getTranManager();
				try {
					tx.beginTransaction();
					QueryCondition qc = new QueryCondition(MedicalHistoryEntity.ID,QueryCondition.in, ids);
					result = dbManager.delByConditionsNoTransaction(MedicalHistoryEntity.class, qc);
					tx.commitAndClose();
				} catch (Exception e) {
					logger.error("数据库提交失败！");
					logger.error(e);
					result = false;
					try {
						tx.rollbackAndClose();
					} catch (Exception ex) {
						logger.error("数据库回滚失败！");
						logger.error(ex);
					}
				}
			}
			return result;
		}

