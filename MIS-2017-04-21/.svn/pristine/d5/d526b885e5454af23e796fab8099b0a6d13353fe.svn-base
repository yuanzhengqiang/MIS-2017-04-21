				  															  					if(diseaseControl!=null){qc.andCondition(new QueryCondition(MedicalHistoryEntity.DISEASE_CONTROL, QueryCondition.eq, diseaseControl));}
		            if(diseaseControl_like!=null){qc.andCondition(new QueryCondition(MedicalHistoryEntity.DISEASE_CONTROL, QueryCondition.like, diseaseControl_like));}
		            if(diseaseControl_isNull!=null){qc.andCondition(new QueryCondition(MedicalHistoryEntity.DISEASE_CONTROL, QueryCondition.isNull, diseaseControl_isNull));}
		            if(diseaseControl_isNotNull!=null){qc.andCondition(new QueryCondition(MedicalHistoryEntity.DISEASE_CONTROL, QueryCondition.isNotNull, diseaseControl_isNotNull));}
				    if(diseaseControl_in!=null){qc.andCondition(new QueryCondition(MedicalHistoryEntity.DISEASE_CONTROL, QueryCondition.in, diseaseControl_in));}
				  															  					if(other!=null){qc.andCondition(new QueryCondition(MedicalHistoryEntity.OTHER, QueryCondition.eq, other));}
		            if(other_like!=null){qc.andCondition(new QueryCondition(MedicalHistoryEntity.OTHER, QueryCondition.like, other_like));}
		            if(other_isNull!=null){qc.andCondition(new QueryCondition(MedicalHistoryEntity.OTHER, QueryCondition.isNull, other_isNull));}
		            if(other_isNotNull!=null){qc.andCondition(new QueryCondition(MedicalHistoryEntity.OTHER, QueryCondition.isNotNull, other_isNotNull));}
				    if(other_in!=null){qc.andCondition(new QueryCondition(MedicalHistoryEntity.OTHER, QueryCondition.in, other_in));}
				  										
				            						if (qc.getQueryNextCondition()!=null) {
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					tx.beginTransaction(); 						
										 	                 				     					     //删除关联信息
						if(delCaseRecord!=null&&delCaseRecord.booleanValue()){
							List<Object> list=dbManager.queryByConditionNoTransaction(MedicalHistoryEntity.class,qc);
							String strIds="";
							if(list!=null&&list.size()>0){
								for(Object obj:list){
									MedicalHistoryEntity entity=(MedicalHistoryEntity)obj;
									strIds += entity.getCaseRecordId()+",";
								}
								strIds = strIds.substring(0, strIds.length()-1);
							}
							if(strIds!=null&&!"".equals(strIds)){
								QueryCondition qc1 = new QueryCondition(CaseRecordEntity.ID, QueryCondition.in, strIds);
								dbManager.delByConditionsNoTransaction(CaseRecordEntity.class, qc1);
							}
							
						}
				     				     				     										result = dbManager.delByConditionsNoTransaction(MedicalHistoryEntity.class,qc);				
					tx.commitAndClose();  
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  
			}
			return result;
		}
		
		
