		/**
		 * 删除记录
		 * 
		 * @param id 主键
		 * @param obj
		 */
		public boolean del(Integer id,Boolean delCreateUser,Boolean delUpdateUser) {
			boolean result = false;
			if (id !=null&&id>0) {
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					tx.beginTransaction(); 					
										 	                 				     					    //删除关联信息
						if(delCreateUser!=null&&delCreateUser.booleanValue()){	
							PersonInfoEntity personInfo = (PersonInfoEntity)dbManager.getById(id, PersonInfoEntity.class);
							if(personInfo!=null&&personInfo.getCreateUserId()!=null){
								dbManager.delNoTransaction(personInfo.getCreateUserId(), UserEntity.class);
							}						
						}
				     				     				     										 	                 				     					    //删除关联信息
						if(delUpdateUser!=null&&delUpdateUser.booleanValue()){	
							PersonInfoEntity personInfo = (PersonInfoEntity)dbManager.getById(id, PersonInfoEntity.class);
							if(personInfo!=null&&personInfo.getUpdateUserId()!=null){
								dbManager.delNoTransaction(personInfo.getUpdateUserId(), UserEntity.class);
							}						
						}
				     				     				     										result = dbManager.delNoTransaction(id, PersonInfoEntity.class);
					tx.commitAndClose(); 
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  
			}
			return result;
		}
		
		/**
		 * 批量删除记录
		 * 
		 * @param ids
		 *            主键 英文逗号间隔
		 * @param obj
		 */
		public boolean del(String ids) {
			boolean result = false;
			if (ids != null && !"".equals(ids.trim())) {
				TransactionManager tx = DbUtils.getTranManager();
				try {
					tx.beginTransaction();
					QueryCondition qc = new QueryCondition(PersonInfoEntity.ID,QueryCondition.in, ids);
					result = dbManager.delByConditionsNoTransaction(PersonInfoEntity.class, qc);
					tx.commitAndClose();
				} catch (Exception e) {
					logger.error("数据库提交失败！");
					logger.error(e);
					result = false;
					try {
						tx.rollbackAndClose();
					} catch (Exception ex) {
						logger.error("数据库回滚失败！");
						logger.error(ex);
					}
				}
			}
			return result;
		}

