		var reqmsg="{'action':'QUERY_PHYSIOLOGICAL_PARAMETERS_HISTORY_LIST_REQUEST', 'order':[{'column':'createTime','type':'desc'}],'page':{'pageno':'" + pagenumber + "','pagesize':'" + pagesize + "'},'content':{";
