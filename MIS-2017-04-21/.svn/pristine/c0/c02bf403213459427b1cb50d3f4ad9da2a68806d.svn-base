	function tableORline(par1,par){ //生理参数页签--par1为1~5，num为1默认表格,0为折线图
		var num = $(par).val();
		num = num %2;
		$("#name"+par1+"-"+num).show();
		num = (num+1) %2;
		$("#name"+par1+"-"+num).hide();
