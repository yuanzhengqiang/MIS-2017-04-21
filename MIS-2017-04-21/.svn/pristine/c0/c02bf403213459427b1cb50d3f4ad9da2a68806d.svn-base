		reqmsg += "\"type_in\":\"1\",";
		var time = "";
		var leftTJ = "";
		if(flag == ""){
			leftTJ = $("#xueya_leftTJ").val();
		}else{
			leftTJ = flag;
			$("#xueya_leftTJ").val(flag);
		}
		if(leftTJ == "一周"){
			time = lastweekTime();
			reqmsg += "\"createTime_ge\":\"" + time + "\",";
			$("#xueya_leftTimeStart").val("");
			$("#xueya_leftTimeEnd").val("");
