//获取本月第一天
function getthisMonthFirstDay(){
	var date = new Date();
    var month = date.getMonth() + 1;
    if (month >= 1 && month <= 9) {
        month = "0" + month;
    }
    return date.getFullYear() + "-" + month + "-" + "01";
}
//获取本月最后一天
function getthisMonthLastDay(){
	var date = new Date();
    var month = date.getMonth() + 1;
	var nextMonthFirstDay = new Date(date.getFullYear(),month,1);
 	var oneDay = 1000*60*60*24;
 	var date2 = new Date(nextMonthFirstDay-oneDay);
	var strDate = date2.getDate();
    return date.getFullYear() + "-" + month + "-" + strDate;
}

//查询健康评估统计信息
function getHealthTongji(){
	var stratTime = getthisMonthFirstDay();
