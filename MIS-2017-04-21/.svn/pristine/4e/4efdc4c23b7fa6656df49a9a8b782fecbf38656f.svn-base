@TableDescription(name = "B_SKIN_LESIONS")
public class SkinLesionsEntity implements java.io.Serializable {
    public final static String ID = "ID";
    public final static String RECOVERY_ID = "RECOVERY_ID";
    public final static String POSITION = "POSITION";
    public final static String POSITION_LONG = "POSITION_LONG";
    public final static String POSITION_WIDE = "POSITION_WIDE";
    public final static String SITE_DEEP = "SITE_DEEP";
    public final static String PHOTO_URL = "PHOTO_URL";
    
    /**
	 * 主键
	 */
	@ColumnDescription(name = "ID", isPrimaryKey = true, isAutoIncrement = true)
	private Integer id;
    /**
	 * 康复评估ID
	 */
	@ColumnDescription(name = "RECOVERY_ID")
	private Integer recoveryId;
    /**
	 * 部位
	 */
	@ColumnDescription(name = "POSITION")
	private String position;
    /**
	 * 部位长
	 */
	@ColumnDescription(name = "POSITION_LONG")
	private BigDecimal positionLong;
    /**
	 * 部位宽
	 */
	@ColumnDescription(name = "POSITION_WIDE")
	private BigDecimal positionWide;
    /**
	 * 部位深
	 */
	@ColumnDescription(name = "SITE_DEEP")
	private BigDecimal siteDeep;
    /**
	 * 图片地址
	 */
	@ColumnDescription(name = "PHOTO_URL")
	private String photoUrl;
        
    public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}
    public Integer getRecoveryId() {
		return recoveryId;
	}

	public void setRecoveryId(Integer recoveryId) {
		this.recoveryId = recoveryId;
	}
    public String getPosition() {
		return position;
	}

	public void setPosition(String position) {
		this.position = position;
	}
    public BigDecimal getPositionLong() {
		return positionLong;
	}

	public void setPositionLong(BigDecimal positionLong) {
		this.positionLong = positionLong;
	}
    public BigDecimal getPositionWide() {
		return positionWide;
	}

	public void setPositionWide(BigDecimal positionWide) {
		this.positionWide = positionWide;
	}
    public BigDecimal getSiteDeep() {
		return siteDeep;
	}

	public void setSiteDeep(BigDecimal siteDeep) {
		this.siteDeep = siteDeep;
	}
    public String getPhotoUrl() {
		return photoUrl;
	}

	public void setPhotoUrl(String photoUrl) {
		this.photoUrl = photoUrl;
	}
        
    /**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="RecoveryEntity"	,joinColumn="ID"	)
			
			
		private RecoveryEntity recovery;
	        
	public RecoveryEntity getRecovery() {
		return recovery;
    }
	
	public void setRecovery(RecoveryEntity recovery) {
		this.recovery = recovery;
	}
			
}
