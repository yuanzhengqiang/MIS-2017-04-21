package fsk.pack.examinationData;

import java.util.HashMap;
import java.util.Map;

import net.sf.json.JSONArray;
import net.sf.json.JSONObject;

import org.apache.log4j.Logger;

import fsk.entity.examinationData.ExaminationDataEntity;

import com.framework.system.db.query.PageList;
 /**   
 * @Title: Pack
 * @Description: 体检数据信息表封装器
 * @author feng.gu
 * @date 2016-10-11 15:23:02
 * @version V1.0   
 *
 */
public class ExaminationDataPack {
	private static Logger logger = Logger.getLogger(ExaminationDataPack.class);
	private static ExaminationDataPack examinationDataPack;

	/**
	 * 获取实例
	 * 
	 * @return
	 */
	public static ExaminationDataPack getInstance() {
		if (examinationDataPack == null) {
			examinationDataPack = new ExaminationDataPack();
		}
		return examinationDataPack;
	}

	public Map<String, Object> pack(int type, String action, Object request,Object entity) {
		// 定义返回参数
		Map<String, Object> packMap = new HashMap<String, Object>();
		if (type == 1) {
			// json
			packMap = this.packByJson(action, request,entity);
		} else if (type == 2) {
			// xml
			packMap = this.packByXml(action, request,entity);
		}
		return packMap;
	}

	private Map<String, Object> packByJson(String action, Object request,Object entity) {
		// 定义返回参数
		Map<String, Object> packMap = new HashMap<String, Object>();
		String actionBack = null;
		String resultBack = null;
		String desBack = null;
		JSONObject pageBack = null;
		JSONObject contentBack = null;
		try {
			if ("save".equals(action)) {
				actionBack="ADD_EXAMINATION_DATA_INFO_RESPONSE";	
				if(request.equals(true)){
					resultBack="100";
					desBack="success";
				}else if(request.equals(false)){
					resultBack="200";
					desBack="failure";
				}   
				ExaminationDataEntity examinationData = (ExaminationDataEntity)entity;
				if(examinationData!=null){
					contentBack = new JSONObject();
					contentBack.put("id", examinationData.getId());
					
																									if(examinationData.getExaminationRecord()!=null){
						contentBack.put("examinationRecordId", examinationData.getExaminationRecord().getId());
					}
																																								if(examinationData.getType1DataDic()!=null){
						contentBack.put("type1DataDicId", examinationData.getType1DataDic().getId());
					}
																																								if(examinationData.getType2DataDic()!=null){
						contentBack.put("type2DataDicId", examinationData.getType2DataDic().getId());
					}
																																																																																														}
			} else if ("getById".equals(action)) {
				actionBack="QUERY_EXAMINATION_DATA_INFO_RESPONSE";		
				resultBack="100";
				desBack="success";
				ExaminationDataEntity examinationData = (ExaminationDataEntity)request;
				if(examinationData!=null){
					contentBack = JSONObject.fromObject(examinationData);
																									if(examinationData.getExaminationRecord()!=null){
						contentBack.put("examinationRecord", JSONObject.fromObject(examinationData.getExaminationRecord()));
					}
																																								if(examinationData.getType1DataDic()!=null){
						contentBack.put("type1DataDic", JSONObject.fromObject(examinationData.getType1DataDic()));
					}
																																								if(examinationData.getType2DataDic()!=null){
						contentBack.put("type2DataDic", JSONObject.fromObject(examinationData.getType2DataDic()));
					}
																																            if(examinationData.getExaminationDataFileList()!=null){
						contentBack.put("examinationDataFileList", JSONArray.fromObject(examinationData.getExaminationDataFileList()));
					}
																																					            if(examinationData.getExaminationDataPhotoList()!=null){
						contentBack.put("examinationDataPhotoList", JSONArray.fromObject(examinationData.getExaminationDataPhotoList()));
					}
																													}
			} else if ("getListByCondition".equals(action)) {
				actionBack="QUERY_EXAMINATION_DATA_LIST_RESPONSE";
				resultBack="100";
				desBack="success";
				PageList pageList = (PageList)request;
				if(pageList!=null&&pageList.getResultList()!=null&&pageList.getResultList().size()>0){
					contentBack = new JSONObject();
					pageBack = new JSONObject();
					JSONArray list = JSONArray.fromObject(pageList.getResultList());
					contentBack.put("examinationDataList", list);
					
					pageBack.put("pageno", pageList.getPageno());
					pageBack.put("pagesize", pageList.getPagesize());
					pageBack.put("recordCount", pageList.getRecordCount());
					pageBack.put("pageCount", pageList.getPageCount());
					
				}
			} else if ("del".equals(action)) {
				actionBack="DEL_EXAMINATION_DATA_INFO_REQUEST";	
				if(request.equals(true)){
					resultBack="100";
					desBack="success";
				}else if(request.equals(false)){
					resultBack="200";
					desBack="failure";
				} 
			} else if ("delList".equals(action)) {
				actionBack="DEL_EXAMINATION_DATA_LIST_REQUEST";	
				if(request.equals(true)){
					resultBack="100";
					desBack="success";
				}else if(request.equals(false)){
					resultBack="200";
					desBack="failure";
				} 
			}
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
			logger.error(e);
		}
		if (actionBack != null && !"".equals(actionBack)) {
			packMap.put("action", actionBack);
		}
		if (resultBack != null&& !"".equals(resultBack)) {
			packMap.put("result", resultBack);
		}
		if (desBack != null&& !"".equals(desBack)) {
			packMap.put("des", desBack);
		}
		if (pageBack != null) {
			packMap.put("page", pageBack);
		}
		if (contentBack != null) {
			packMap.put("content", contentBack);
		}
		return packMap;
	}

	private Map<String, Object> packByXml(String action, Object request,Object entity) {
		Map<String, Object> packMap = new HashMap<String, Object>();
		return packMap;
	}

}
