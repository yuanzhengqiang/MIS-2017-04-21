	   /**
		 * 保存记录
		 * 
		 * @param obj
		 */
		public boolean save(OlderContactEntity olderContact) {			
			boolean result =false;
			if(olderContact!=null){
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					 tx.beginTransaction();					 					 
					 					 				     				     					     //关联信息保存
					     OlderEntity older = olderContact.getOlder();
						 if(older!=null){						   
							 dbManager.saveNoTransaction(older);
							 olderContact.setOlderId(older.getId());
						 }						 
				     				     				     					 					 result=dbManager.saveNoTransaction(olderContact);	
					 tx.commitAndClose();  
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {				    
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  			
			}	
			return result;
		}
		
		/**
		 * 批量保存记录
		 * 
		 * @param list
		 */
		public boolean saveList(List<OlderContactEntity> olderContactList) {
			boolean result = false;
			if (olderContactList != null && olderContactList.size() > 0) {
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					tx.beginTransaction(); 
					for(OlderContactEntity olderContact:olderContactList){
						if(olderContact!=null){												  								 
								 									 					                 								     								         //关联信息保存
									     OlderEntity older = olderContact.getOlder();
										 if(older!=null){						   
											 dbManager.saveNoTransaction(older);
											 olderContact.setOlderId(older.getId());
										 }									 
								     								     								     								 								 result=dbManager.saveNoTransaction(olderContact);								 			
						}	
					}
				    tx.commitAndClose(); 
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  
			}
			return result;
		}
		
