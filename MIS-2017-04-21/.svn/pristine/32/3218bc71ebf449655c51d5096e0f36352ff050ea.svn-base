		var htmlcode = "<a href=\""+headUrl+"/wechatDetail.do?main&roleIds="+id+"\" target=\"_blank\" tag1=\""+id+"\" tag2=\""+openid+"\">";
		htmlcode += name;
		htmlcode += "</a>";
		$("#wechatName").html(htmlcode);
		$("#closeWeChat").click();
    	
    }
    
