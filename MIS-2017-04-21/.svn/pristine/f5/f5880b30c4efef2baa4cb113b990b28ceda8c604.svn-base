	   /**
		 * 保存记录
		 * 
		 * @param obj
		 */
		public boolean save(OlderPrincipalEmployeeEntity olderPrincipalEmployee) {			
			boolean result =false;
			if(olderPrincipalEmployee!=null){
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					 tx.beginTransaction();					 					 
					 					 				     				     					     //关联信息保存
					     OlderEntity older = olderPrincipalEmployee.getOlder();
						 if(older!=null){						   
							 dbManager.saveNoTransaction(older);
							 olderPrincipalEmployee.setOlderId(older.getId());
						 }						 
				     				     				     					 					 				     				     					     //关联信息保存
					     EmployeeEntity principalEmployee = olderPrincipalEmployee.getPrincipalEmployee();
						 if(principalEmployee!=null){						   
							 dbManager.saveNoTransaction(principalEmployee);
							 olderPrincipalEmployee.setPrincipalEmployeeId(principalEmployee.getId());
						 }						 
				     				     				     					 					 result=dbManager.saveNoTransaction(olderPrincipalEmployee);	
					 tx.commitAndClose();  
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {				    
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  			
			}	
			return result;
		}
		
		/**
		 * 批量保存记录
		 * 
		 * @param list
		 */
		public boolean saveList(List<OlderPrincipalEmployeeEntity> olderPrincipalEmployeeList) {
			boolean result = false;
			if (olderPrincipalEmployeeList != null && olderPrincipalEmployeeList.size() > 0) {
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					tx.beginTransaction(); 
					for(OlderPrincipalEmployeeEntity olderPrincipalEmployee:olderPrincipalEmployeeList){
						if(olderPrincipalEmployee!=null){												  								 
								 									 					                 								     								         //关联信息保存
									     OlderEntity older = olderPrincipalEmployee.getOlder();
										 if(older!=null){						   
											 dbManager.saveNoTransaction(older);
											 olderPrincipalEmployee.setOlderId(older.getId());
										 }									 
								     								     								     								 									 					                 								     								         //关联信息保存
									     EmployeeEntity principalEmployee = olderPrincipalEmployee.getPrincipalEmployee();
										 if(principalEmployee!=null){						   
											 dbManager.saveNoTransaction(principalEmployee);
											 olderPrincipalEmployee.setPrincipalEmployeeId(principalEmployee.getId());
										 }									 
								     								     								     								 								 result=dbManager.saveNoTransaction(olderPrincipalEmployee);								 			
						}	
					}
				    tx.commitAndClose(); 
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  
			}
			return result;
		}
		
