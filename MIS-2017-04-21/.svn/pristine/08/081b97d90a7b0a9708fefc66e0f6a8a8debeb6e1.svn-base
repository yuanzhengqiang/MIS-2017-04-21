	/**
	 * 保存记录
	 * 
	 * @param obj
	 */
	public boolean save(ExaminationRecordFileEntity examinationRecordFile) {
		boolean result = false;
		if (examinationRecordFile != null) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				// 关联信息保存
				ExaminationRecordEntity examinationRecord = examinationRecordFile
						.getExaminationRecord();
				if (examinationRecord != null) {
					dbManager.saveNoTransaction(examinationRecord);
					examinationRecordFile
							.setExaminationRecordId(examinationRecord.getId());
				}
				result = dbManager.saveNoTransaction(examinationRecordFile);
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}

	/**
	 * 批量保存记录
	 * 
	 * @param list
	 */
	public boolean saveList(
			List<ExaminationRecordFileEntity> examinationRecordFileList) {
		boolean result = false;
		if (examinationRecordFileList != null
				&& examinationRecordFileList.size() > 0) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				for (ExaminationRecordFileEntity examinationRecordFile : examinationRecordFileList) {
					if (examinationRecordFile != null) {
						// 关联信息保存
						ExaminationRecordEntity examinationRecord = examinationRecordFile
								.getExaminationRecord();
						if (examinationRecord != null) {
							dbManager.saveNoTransaction(examinationRecord);
							examinationRecordFile
									.setExaminationRecordId(examinationRecord
											.getId());
						}
						result = dbManager
								.saveNoTransaction(examinationRecordFile);
					}
				}
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}

