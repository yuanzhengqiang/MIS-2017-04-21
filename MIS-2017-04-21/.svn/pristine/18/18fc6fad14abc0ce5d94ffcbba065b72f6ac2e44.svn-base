package fsk.handler.employeePositionalTitle;

import java.io.OutputStream;
import java.util.List;
import java.util.Map;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import net.sf.json.JSONObject;

import org.apache.log4j.Logger;

import fsk.entity.employeePositionalTitle.EmployeePositionalTitleEntity;
import fsk.pack.employeePositionalTitle.EmployeePositionalTitlePack;
import fsk.parse.employeePositionalTitle.EmployeePositionalTitleParse;
import fsk.service.employeePositionalTitle.EmployeePositionalTitleService;
import com.framework.system.common.service.SystemService;

import com.framework.system.business.handler.BaseHandler;
import com.framework.system.db.query.OrderVO;

 /**   
 * @Title: Handler
 * @Description: 员工职称关联表业务处理器
 * @author feng.gu
 * @date 2016-06-21 08:39:50
 * @version V1.0   
 *
 */
public class EmployeePositionalTitleHandler extends BaseHandler{
	private static Logger logger = Logger.getLogger(EmployeePositionalTitleService.class);
	/**
	 * 解析器
	 */
	private EmployeePositionalTitleParse employeePositionalTitleParse = EmployeePositionalTitleParse.getInstance();
	/**
	 * 业务处理器
	 */
	private EmployeePositionalTitleService employeePositionalTitleService = EmployeePositionalTitleService.getInstance();
	/**
	 * 封装器
	 */
	private EmployeePositionalTitlePack employeePositionalTitlePack = EmployeePositionalTitlePack.getInstance();

	private static EmployeePositionalTitleHandler employeePositionalTitleHandler;
	
	/**
	 * 系统服务类
	 */
	private SystemService systemService = SystemService.getInstance();

	/**
	 * 获取实例
	 * 
	 * @return
	 */
	public static EmployeePositionalTitleHandler getInstance() {
		if (employeePositionalTitleHandler == null) {
			employeePositionalTitleHandler = new EmployeePositionalTitleHandler();
		}
		return employeePositionalTitleHandler;
	}

	/**
	 * 
	 * @param type 1-json 2-xml
	 * @param entityName
	 * @param command
	 * @param reqStr
	 * @param request
	 * @param response
	 */
	public String doHandler(int type,String command,String reqStr, HttpServletRequest request,
			HttpServletResponse response) {	
		logger.debug("请求消息："+reqStr);
		String resultStr="";
		try {
			// 解析
			Map<String, Object> parseMap = employeePositionalTitleParse.parse(type,command,reqStr,request);		
			String action = (String) parseMap.get("action");
			EmployeePositionalTitleEntity employeePositionalTitle = (EmployeePositionalTitleEntity) parseMap.get("employeePositionalTitle");
			List<EmployeePositionalTitleEntity> employeePositionalTitleList = (List<EmployeePositionalTitleEntity>) parseMap.get("employeePositionalTitleList");
			Integer id = (Integer) parseMap.get("id");			
			Map<String, Object> queryMap = (Map<String, Object>) parseMap.get("queryMap");
			int pageno = (Integer) parseMap.get("pageno");
			int pagesize = (Integer) parseMap.get("pagesize");
			List<OrderVO> orderList = (List<OrderVO>) parseMap.get("orderList");
			
															Boolean employeeShow = (Boolean) parseMap.get("employeeShow");
			Boolean delEmployee = (Boolean) parseMap.get("delEmployee");
																								Boolean positionalTitleShow = (Boolean) parseMap.get("positionalTitleShow");
			Boolean delPositionalTitle = (Boolean) parseMap.get("delPositionalTitle");
															
			// 业务处理
			Object result=null;
			if ("save".equals(action)) {
				result = employeePositionalTitleService.save(employeePositionalTitle);
			} else if ("saveList".equals(action)) {
				result = employeePositionalTitleService.saveList(employeePositionalTitleList);
			} else if ("getById".equals(action)) {
				result = employeePositionalTitleService.getById(id,employeeShow,positionalTitleShow);
			} else if ("getListByCondition".equals(action)) {
			    //根据数据权限 增加查询条件
				queryMap = systemService.addDataRule("EmployeePositionalTitleEntity",queryMap,request);
				result = employeePositionalTitleService.getListByCondition(queryMap,orderList, pageno,
						pagesize,employeeShow,positionalTitleShow);
			} else if ("del".equals(action)) {
				result = employeePositionalTitleService.del(id,delEmployee,delPositionalTitle);
			} else if ("delList".equals(action)) {
				result = employeePositionalTitleService.delList(queryMap,delEmployee,delPositionalTitle);
			}
			// 封装
			Map<String, Object> packMap = employeePositionalTitlePack.pack(type,action,result,employeePositionalTitle);
			String actionBack = (String)packMap.get("action");
			String resultBack = (String)packMap.get("result");
			String desBack = (String)packMap.get("des");
			JSONObject pageBack = (JSONObject)packMap.get("page");
			JSONObject contentBack = (JSONObject)packMap.get("content");
			
			JSONObject repJson = new JSONObject();
			if(actionBack!=null&&!"".equals(actionBack)){
				repJson.put("action", actionBack);
			}
			if(resultBack!=null&&!"".equals(resultBack)){
				repJson.put("result", resultBack);
			}
			if(desBack!=null&&!"".equals(desBack)){
				repJson.put("des", desBack);
			}
			if(pageBack!=null){
				repJson.put("page", pageBack);
			}
			if(contentBack!=null&&!"".equals(contentBack)){
				repJson.put("content", contentBack);
			}
			logger.debug("返回消息："+repJson.toString());
			resultStr = repJson.toString();
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
			logger.error(e);
		}
		return resultStr;
	}
}
