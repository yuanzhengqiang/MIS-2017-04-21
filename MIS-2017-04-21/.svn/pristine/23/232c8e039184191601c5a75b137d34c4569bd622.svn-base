					alert("查询失败");
				}
			},
			error : function() {
				alert("error");
			}
		});
	}
	
    //编辑时调用
    function query(id){
    	var reqmsg="{\"action\":\"QUERY_EXAMINATION_RECORD_INFO_REQUEST\",\"content\":{";
    	reqmsg += "\"id\":" + id + ",";
		reqmsg += "\"olderShow\":\"true\",";
		reqmsg += "\"examinationRecordFileListShow\":\"true\",";
		reqmsg += "\"examinationRecordPhotoListShow\":\"true\",";
		reqmsg += "\"examinationDataListShow\":\"true\"}}";
		
		jQuery.ajax({
	         type : "post",
	         async:true,
	         url : "examinationRecord.do?handler",
	         dataType : "json",
	         data: {
	             "reqmsg":reqmsg
	         },
	         success : function(data){
	             if(data.des=="success"){
	            	 changeData(data);
	           	
	             }else if(data.des=="failure"){
	                 alert("获取信息失败");
	             }else{
	             	 alert(data.des);
	             }
	         },
	         error:function(){
		         alert("error");
	         }
	     });
    }
    
