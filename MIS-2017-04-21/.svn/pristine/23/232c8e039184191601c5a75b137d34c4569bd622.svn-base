	//文件名去后缀
	function delSuffix(file) {
		var fileNameNumber = file.lastIndexOf(".");
		var fileName = file.substring(0, fileNameNumber);//文件名称
		return fileName;
	}

	//时间添加格式201610100101 / 2016-10-10 01:01
	function formatDate(time) {
		time = time.substring(0, 4) + "-" + time.substring(4, 6) + "-" + 
				time.substring(6, 8) + " " + time.substring(8, 10) + ":" + 
				time.substring(10, 12);
		return time;
	}

