	//点击文件名下载
	$(document).on("click", ".aDownload", function() {
		var id = $(this).parents("tr").attr("id");//或取当前文件id
		if(id != null && id !="" && id !="undefined"){//若不存在id则为临时文件下载
			id = parseInt(id);
			var tbodyId = $(this).parents("tbody").attr("id");//获取当前点击的tbody元素的id
			if(tbodyId != null && tbodyId == "baogao_list" ){//如果id=baogao_list则当前点击的是报告中的文件
				$("#baogaoFileId").val(id);
				$("#baogaoIdForm").submit();
			 }else{//如果id不存在t则当前点击的是体检项目中的文件
		     	$("#listFileId").val(id);
				$("#listIdForm").submit();
			 }
		 }else{//若存在id则为已保存文件下载
			var url = "";
			url = $(this).parent().next().next().next().text();
			var file = $(this).text();
			var fileNameNumber = file.lastIndexOf(".");
			var name = $(this).parent().next().text();
			var fileSuf = file.substring(fileNameNumber);//文件后缀
			var fileName = name + fileSuf;
			fileName = encodeURIComponent(fileName);
			var tbodyId = $(this).parents("tbody").attr("id");//获取当前点击的tbody元素的id
			if(tbodyId != null && tbodyId == "baogao_list" ){//如果id=baogao_list则当前点击的是报告中的文件
				$("#baogaoFileName").val(fileName);
				$("#baogaoFileUrl").val(url);
				$("#baogaoForm").submit();
			}else{//如果id不存在t则当前点击的是体检项目中的文件
		     	$("#listFileName").val(fileName);
		     	$("#listFileUrl").val(url);
				$("#listForm").submit();
			}
		 }
	});

