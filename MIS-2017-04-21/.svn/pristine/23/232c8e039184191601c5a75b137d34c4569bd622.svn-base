						if (data.content.gender != null && data.content.gender != "") {
							sex = data.content.gender;
						}
						if (data.content.memberNum != null && data.content.memberNum != "") {
							huiyuanhao = data.content.memberNum;
						}
						if (data.content.idnumber != null && data.content.idnumber != "") {
							idnumber = data.content.idnumber;
						}
						$("#name_header").html(name + "体检报告");
						$("#name_sex_header").html(name + "/" + sex);
						$("#huiyuan_idnumber_header").html(huiyuanhao + "/" + idnumber);
						
						$("#oldername_hidden").val(name);
