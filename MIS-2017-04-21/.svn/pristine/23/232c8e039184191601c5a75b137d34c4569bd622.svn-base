		fileHtml += "<tr class=\"fileContent\" id=\"\"><td><span class=\"aDownload\">" + file + "</span></td><td>" + wenjianname + "</td><td>" + wenjiandes + "</td><td class=\"TPhide\">" + url + "</td>";
