	function savewenjian() {
		var file = $("#hideFile").val();//文件名
		if(file == ""){
			alert("请选择要上传的文件");
			return;
		}
		var wenjianname = $("#wenjianname").val();//名称
		if (wenjianname == "") {
			alert("名称不能为空");
			return;
		}
		var filePath, fileSuf;
		var filePathNumber = file.lastIndexOf("\\") + 1;
		var fileNameNumber = file.lastIndexOf(".");
		if (filePathNumber != -1) {
			filePath = file.substring(0, filePathNumber);//文件路径
