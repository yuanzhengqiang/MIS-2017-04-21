			table4.conclusion = conclusion;
			table4.aminationTime = tijianshijian;
			table4.createTime = jilushijian;
			if (fileDetails != "") {
				table4.examinationDataFileList = fileDetails;
			}
			if (picDetails != "") {
				table4.examinationDataPhotoList = picDetails;
			}

			tables4 += JSON.stringify(table4);
			}
		});
		tables4 = tables4.replace(/}{/g, "},{");
		if (tables4 != "") {
			examinationDataList += tables4;
		} else {
			examinationDataList = examinationDataList.substr(0,
					examinationDataList.length - 1);
		}

		var reqmsg = "{\"action\":\"ADD_EXAMINATION_RECORD_INFO_REQUEST\",\"content\":{";
		if(mainId){
			reqmsg += "\"id\":" + parseInt(mainId) + ",";
		}
		reqmsg += "\"olderId\":" + olderId + ",";
		reqmsg += "\"olderName\":\"" + olderName + "\",";
		if(login_id != ""){
			reqmsg += "\"createEmployeeId\":" + login_id + ",";
		}
		
		reqmsg += "\"createEmployeeName\":\"" + login_Name + "\",";
		if (tijianage != null && tijianage != "") {
			reqmsg += "\"olderAge\":\"" + tijianage + "\",";
		}
		if (tijianjigou != null && tijianjigou != "") {
			reqmsg += "\"examinationOrganization\":\"" + tijianjigou + "\",";
		}
		if (tijianshijian != null && tijianshijian != "") {
			reqmsg += "\"examinationTime\":\"" + tijianshijian + "\",";
		}
		if (jilushijian != null && jilushijian != "") {
			reqmsg += "\"createTime\":\"" + jilushijian + "\",";
		}
		if (lianxidianhua != null && lianxidianhua != "") {
			reqmsg += "\"contactTel\":\"" + lianxidianhua + "\",";
		}
		if (jielun != null && jielun != "") {
			jielun = JSON.stringify(jielun);
			if (jielun.length > 500) {
				alert("报告中结论超出字数限制500字");
				return;
			}
			reqmsg += "\"conclusion\":" + jielun + ",";
		}
		if (jianyi != null && jianyi != "") {
			jianyi = JSON.stringify(jianyi);
			if (jianyi.length > 500) {
				alert("报告中建议超出字数限制500字");
				return;
			}
			reqmsg += "\"suggestion\":" + jianyi + ",";
		}

		//报告的文件详情
		var examinationRecordFileList = "";
		$("#baogao_list tr").each(function(index) {
			var fileId = $(this).attr("id");
			var file = $(this).find("td").eq(0).text();//文件名
			var fileNameNumber = file.lastIndexOf(".");
			var fileSuf = file.substring(fileNameNumber);//文件后缀
			var fileName = $(this).find("td").eq(1).text();//文件名称
			var name = fileName + fileSuf;
			var fileDes = $(this).find("td").eq(2).text();//备注
			var fileUrl = $(this).find("td").eq(3).text();//url
			var showIndex = ++index;//显示顺序
			if (file != "") {
				var fileDetail = JSON.parse("{}");
				if(fileId){
					fileDetail.id = parseInt(fileId);
				}
				fileDetail.url = fileUrl;
				fileDetail.name = name;
				fileDetail.des = fileDes;
				fileDetail.showIndex = showIndex;
				fileDetail.createPerson = login_Name;
				fileDetail.createTime = createTime;

				examinationRecordFileList += JSON.stringify(fileDetail);
			}
		});
		examinationRecordFileList = examinationRecordFileList.replace(/}{/g,
				"},{");
		if (examinationRecordFileList != "") {
			examinationRecordFileList = "[" + examinationRecordFileList + "],";
			reqmsg += "\"examinationRecordFileList\":"
					+ examinationRecordFileList;
		}else{
			examinationRecordFileList = "[],";
			reqmsg += "\"examinationRecordFileList\":"
					+ examinationRecordFileList;
		}

		//报告的图片详情
		var tupianFlag = 0;
		for ( var i = 1; i < 7; i++) {
			if ($("#hide_tupian" + i).val() != null
					&& $("#hide_tupian" + i).val() != "") {
				tupianFlag++;
			}
		}
		if (tupianFlag != 0) {
			tupianFlag = 1;
			reqmsg += "\"examinationRecordPhotoList\":[";
			for ( var i = 1; i < 7; i++) {
				var picId = $("#baogao" + i).val();
				if ($("#hide_tupian" + i).val() != null&& $("#hide_tupian" + i).val() != "") {
					if(picId != ""){
						reqmsg += "{\"id\":" + parseInt(picId) + ",";
						reqmsg += "\"url\":\"" + $("#hide_tupian" + i).val() + "\",";
					}else{
						reqmsg += "{\"url\":\"" + $("#hide_tupian" + i).val() + "\",";
					}
					reqmsg += "\"name\":\"" + $("#tpTitle" + i).html() + "\",";
					reqmsg += "\"des\":\"" + $("#hide_tpmiaoshu" + i).val() + "\",";
					reqmsg += "\"createPerson\":\"" + login_Name + "\",";
					reqmsg += "\"createTime\":\"" + getNowFormatDate() + "\",";
					reqmsg += "\"showIndex\":" + tupianFlag + "}";
					tupianFlag++;
				}
				reqmsg = reqmsg.replace(/}{/g, "},{");
			}
			reqmsg += "],";
		}else{
			reqmsg += "\"examinationRecordPhotoList\":[],";
		}

		//项目列表表单详情
		var examinationDataList = tables1 + tables2 + tables3 + tables4;
		if (examinationDataList)
			examinationDataList = examinationDataList.replace(/\\\"/g, '\"').replace(/\"\[/g, "\[").replace(/\]\"/g, "\]").replace(/}{/g, "},{");
		if (examinationDataList != "") {
			examinationDataList = "[" + examinationDataList + "]";
			reqmsg += "\"examinationDataList\":" + examinationDataList;
		} else {
			reqmsg += "\"examinationDataList\":" + "[]";
		}
		reqmsg += "}}";
		var flag = "";
		$("#liList li").each(function(index) {
			var liClass = $(this).attr("class");
			if(liClass == "active"){
				flag = index;
			}
		});
		
		jQuery.ajax({
			type : "post",
			async : true,
			url : "examinationRecord.do?handler",
			dataType : "json",
			data : {
				"reqmsg" : reqmsg
			},
			success : function(data) {
				if (data.des == "success") {
					alert("保存成功");
					var id = data.content.id;
					var path = "<%=request.getContextPath()%>";
	              	var url=path+"/examinationRecord.do?mainXQ&id="+id + "&flag=" + flag ;
	              	window.location.replace(url);
              }else if(data.des=="failure"){
                 alert("保存失败");
              }else{
              	alert(data.des);
              }
          },
          error:function(){
	           alert("error");
          }
     	});
	}
	
	/******************************  模态框中的图片  *************************************** */
	//添加图片
	function addTupian(){
		var weizhi = 1;
		for(var i=1;i<7;i++){
			if($("#fptPhoto"+i).attr("name") == "hide"){
				weizhi = i;
				break;
			}
		}
		$("#weizhiId").val(weizhi);
		$("#picupload"+weizhi).val("");
		$("#picupload"+weizhi).click();
		
		$("#chargeWhichPhotoWeizhi").val("1");
	}
	
	//上传图片
	function upLoadPic2(weizhi) {
		$("#chargeWhichPhotoWeizhi").val("1");
		$("#picupload"+weizhi).click();
	}
	//上传图片
	function upLoadPic(weizhi,flag) {
		$("#weizhiId").val(weizhi);
		var currPic = $("#picupload"+weizhi).val();
		if (currPic != null && currPic != "") {
			var ajax_option = {
				async:false,
				data:{},
				success:function(data) {
					if (data != null && data != "") {
						$("#srcPath").val(data); 
						//$("#touxiangId2").attr("src", data);  //将当前图片设为上传的图片
						if(flag == "flag"){
							openImage(data,weizhi);
						}else{
							openImage(data,'');
						}
					} else {
						alert("图片上传失败");
					}
				}
			};
			$("#picuploadForm"+weizhi).ajaxSubmit(ajax_option);
		} else {
			//alert("请选择图片");
		}
	}
	
	function openImage(data,weizhi){
		if(weizhi != null && weizhi !=""){
			var tpTitles = $("#tpTitles" + weizhi).html();
			var hideTpMiaoshu = $("#hideTpMiaoshu" + weizhi).val();
			window.open ("view/imageCut/jquery-image-cut/image.jsp?flag=jkbg&tpTitles=" + tpTitles +"&tpMiaoshu=" + hideTpMiaoshu + "&" + data, "newwindow", "height=550, width=1100, top=200, left=450, toolbar=no, menubar=no, scrollbars=no, resizable=no,location=no, status=no");
		}else{
			window.open ("view/imageCut/jquery-image-cut/image.jsp?flag=jkbg&"+data, "newwindow", "height=550, width=1100, top=200, left=450, toolbar=no, menubar=no, scrollbars=no, resizable=no,location=no, status=no");
		}
	}
	//上传裁剪后图片
	function upLoadPicCut() {
		
		var weizhi = $("#weizhiId").val();
		$("#srcPath2").val($("#srcPath").val());
		$("#type2").val($("#type").val());
		$("#X1").val($("#x1").val());
		$("#X2").val($("#x2").val());
		$("#Y1").val($("#y1").val());
		$("#Y2").val($("#y2").val());
		var ajax_option = {
			async:false,
			data:{},
			success:function(data) {
				if (data != null && data != "") {
					var result = JSON.parse(data);
					$("#fptPhoto"+weizhi).removeClass("TPhide");
				 	$("#fptPhoto"+weizhi).attr("name","show");
					if(weizhi == 6){
						$("#fptPhotoAdd").attr("style","display:none;");
					}
					$("#hidettupian"+weizhi).val(result.base64);
					$("#ptPhoto"+weizhi).attr("src", result.url);  //将当前图片设为上传的图片
					$("#bigptPhoto"+weizhi).attr("href", result.url);//设置大图
					
					$("#tpTitles"+weizhi).html($("#photoTitle").val());
					var photoDes = $("#photoDes").val();
					if(photoDes != null && photoDes != ""){
						if(photoDes.length > 25){
							$("#tpMiaoshu"+weizhi).html(photoDes.substr(0,25) + "...");
						}else{
							$("#tpMiaoshu"+weizhi).html(photoDes);
						}
					}
					$("#hideTpMiaoshu"+weizhi).val(photoDes);
					
					$("#bianjiTp"+weizhi).attr("onclick","upLoadPic('" + weizhi + "','flag')");
					
					var evObj = document.createEvent('HTMLEvents');
					evObj.initEvent('resize', true, true);
					window.dispatchEvent(evObj);
				} else {
					alert("图片上传失败");
				}
			}
		};
		$("#imageSubmit").ajaxSubmit(ajax_option);
	}
	
	function delTupian(weizhi){
		$("#hidettupian"+weizhi).val("");
		$("#ptPhoto"+weizhi).attr("src", "");
		$("#fptPhoto"+weizhi).attr("name","hide");
		$("#fptPhoto"+weizhi).addClass("TPhide");
		$("#tpTitles"+weizhi).html("");
		$("#tpMiaoshu"+weizhi).html("");
	}
	
	//判断裁剪的图片是模态框中还是报告中
	$(document).on("click", "#sure_SC", function () {
		var flag = $("#chargeWhichPhotoWeizhi").val();
		if(flag == "1"){
			upLoadPicCut();
		}else{
			uploadPic_cut();
		}
    });	
	
	/*
	 * 文字过长缩略显示
	 */
	function formateDetail(par){
	    if (par != null && par.length >50) {
	        return par.substring(0, 50) + "..." ;
	    }else {
	        return par;
	    }
	}
   	
	</script>
