	/**
	 * 删除记录
	 * 
	 * @param id
	 *            主键
	 * @param obj
	 */
	public boolean del(Integer id, Boolean delParentDataDic,
			Boolean delChildDataDicList, Boolean delCreateUser,
			Boolean delUpdateUser) {
		boolean result = false;
		if (id != null && id > 0) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				// 删除关联信息
				if (delParentDataDic != null && delParentDataDic.booleanValue()) {
					DataDicEntity dataDic = (DataDicEntity) dbManager.getById(
							id, DataDicEntity.class);
					if (dataDic.getParentId() != null) {
						dbManager.delNoTransaction(dataDic.getParentId(),
								DataDicEntity.class);
					}
				}
				// 删除关联信息
				if (delChildDataDicList != null
						&& delChildDataDicList.booleanValue()) {
					QueryCondition qc = new QueryCondition(
							DataDicEntity.PARENT_ID, QueryCondition.eq, id);
					dbManager.delByConditionsNoTransaction(DataDicEntity.class,
							qc);
				}
				// 删除关联信息
				if (delCreateUser != null && delCreateUser.booleanValue()) {
					DataDicEntity dataDic = (DataDicEntity) dbManager.getById(
							id, DataDicEntity.class);
					if (dataDic != null && dataDic.getCreateUserId() != null) {
						dbManager.delNoTransaction(dataDic.getCreateUserId(),
								UserEntity.class);
					}
				}
				// 删除关联信息
				if (delUpdateUser != null && delUpdateUser.booleanValue()) {
					DataDicEntity dataDic = (DataDicEntity) dbManager.getById(
							id, DataDicEntity.class);
					if (dataDic != null && dataDic.getUpdateUserId() != null) {
						dbManager.delNoTransaction(dataDic.getUpdateUserId(),
								UserEntity.class);
					}
				}
				result = dbManager.delNoTransaction(id, DataDicEntity.class);
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}

	/**
	 * 批量删除记录
	 * 
	 * @param ids
	 *            主键 英文逗号间隔
	 * @param obj
	 */
	public boolean del(String ids) {
		boolean result = false;
		if (ids != null && !"".equals(ids.trim())) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				QueryCondition qc = new QueryCondition(DataDicEntity.ID,
						QueryCondition.in, ids);
				result = dbManager.delByConditionsNoTransaction(
						DataDicEntity.class, qc);
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}

