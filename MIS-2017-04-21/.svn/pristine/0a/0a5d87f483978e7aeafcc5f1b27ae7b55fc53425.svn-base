	/**
	 * 根据id读取记录
	 * 
	 * @param id
	 *            主键
	 * @param parentDataDicShow
	 *            是否查询关联信息
	 * @param childDataDicListShow
	 *            是否查询关联信息
	 * @param createUserShow
	 *            是否查询关联信息
	 * @param updateUserShow
	 *            是否查询关联信息
	 * @param obj
	 */
	public DataDicEntity getById(Integer id, Boolean parentDataDicShow,
			Boolean childDataDicListShow, Boolean createUserShow,
			Boolean updateUserShow) {
		DataDicEntity obj = null;
		if (id != null) {
			obj = (DataDicEntity) dbManager.getById(id, DataDicEntity.class);
			// 查询关联内容
			if (parentDataDicShow != null && parentDataDicShow.booleanValue()
					&& obj != null && obj.getParentId() > 0) {
				DataDicEntity dataDic = (DataDicEntity) dbManager.getById(
						obj.getParentId(), DataDicEntity.class);
				obj.setParentDataDic(dataDic);
			}
			// 查询关联内容
			if (childDataDicListShow != null
					&& childDataDicListShow.booleanValue() && obj != null
					&& obj.getId() > 0) {
				List<Object> list = dbManager.searchListByColumn(
						DataDicEntity.class, DataDicEntity.PARENT_ID,
						String.valueOf(id));
				if (list != null && list.size() > 0) {
					List<DataDicEntity> childDataDicList = new ArrayList<DataDicEntity>();
					for (Object o : list) {
						childDataDicList.add((DataDicEntity) o);
					}
					obj.setChildDataDicList(childDataDicList);
				}
			}
			// 查询关联内容
			if (createUserShow != null && createUserShow.booleanValue()
					&& obj != null && obj.getCreateUserId() > 0) {
				UserEntity createUser = (UserEntity) dbManager.getById(
						obj.getCreateUserId(), UserEntity.class);
				obj.setCreateUser(createUser);
			}
			// 查询关联内容
			if (updateUserShow != null && updateUserShow.booleanValue()
					&& obj != null && obj.getUpdateUserId() > 0) {
				UserEntity updateUser = (UserEntity) dbManager.getById(
						obj.getUpdateUserId(), UserEntity.class);
				obj.setUpdateUser(updateUser);
			}
		}
		return obj;
	}

