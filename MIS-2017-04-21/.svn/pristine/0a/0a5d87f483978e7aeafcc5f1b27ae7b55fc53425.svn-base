	/**
	 * 保存记录
	 * 
	 * @param obj
	 */
	public boolean save(DataDicEntity dataDic) {
		boolean result = false;
		if (dataDic != null) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				// 关联信息保存
				DataDicEntity parentDataDic = dataDic.getParentDataDic();
				if (parentDataDic != null) {
					dbManager.saveNoTransaction(parentDataDic);
					dataDic.setParentId(parentDataDic.getId());
				}
				// 关联信息保存
				if (dataDic.getChildDataDicList() != null
						&& dataDic.getChildDataDicList().size() > 0) {
					for (DataDicEntity dataDicEntity : dataDic
							.getChildDataDicList()) {
						dbManager.saveNoTransaction(dataDicEntity);
					}
				}
				// 关联信息保存
				UserEntity createUser = dataDic.getCreateUser();
				if (createUser != null) {
					dbManager.saveNoTransaction(createUser);
					dataDic.setCreateUserId(createUser.getId());
				}
				// 关联信息保存
				UserEntity updateUser = dataDic.getUpdateUser();
				if (updateUser != null) {
					dbManager.saveNoTransaction(updateUser);
					dataDic.setUpdateUserId(updateUser.getId());
				}
				result = dbManager.saveNoTransaction(dataDic);
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}

	/**
	 * 批量保存记录
	 * 
	 * @param list
	 */
	public boolean saveList(List<DataDicEntity> dataDicList) {
		boolean result = false;
		if (dataDicList != null && dataDicList.size() > 0) {
			TransactionManager tx = DbUtils.getTranManager();
			try {
				tx.beginTransaction();
				for (DataDicEntity dataDic : dataDicList) {
					if (dataDic != null) {
						// 关联信息保存
						DataDicEntity parentDataDic = dataDic
								.getParentDataDic();
						if (parentDataDic != null) {
							dbManager.saveNoTransaction(parentDataDic);
							dataDic.setParentId(parentDataDic.getId());
						}
						// 关联信息保存
						if (dataDic.getChildDataDicList() != null
								&& dataDic.getChildDataDicList().size() > 0) {
							for (DataDicEntity dataDicEntity : dataDic
									.getChildDataDicList()) {
								dbManager.saveNoTransaction(dataDicEntity);
							}
						}
						// 关联信息保存
						UserEntity createUser = dataDic.getCreateUser();
						if (createUser != null) {
							dbManager.saveNoTransaction(createUser);
							dataDic.setCreateUserId(createUser.getId());
						}
						// 关联信息保存
						UserEntity updateUser = dataDic.getUpdateUser();
						if (updateUser != null) {
							dbManager.saveNoTransaction(updateUser);
							dataDic.setUpdateUserId(updateUser.getId());
						}
						result = dbManager.saveNoTransaction(dataDic);
					}
				}
				tx.commitAndClose();
			} catch (Exception e) {
				logger.error("数据库提交失败！");
				logger.error(e);
				result = false;
				try {
					tx.rollbackAndClose();
				} catch (Exception ex) {
					logger.error("数据库回滚失败！");
					logger.error(ex);
				}
			}
		}
		return result;
	}

