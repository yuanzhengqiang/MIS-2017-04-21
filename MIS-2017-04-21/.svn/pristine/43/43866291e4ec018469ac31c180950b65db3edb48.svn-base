/* base */
*{
    margin: 0px;
    padding: 0px;
	color:#000;
}
body{
    
	font-family: "Microsoft YaHei" ! important;	
}

img{
	border:none;
}

a{
	/* color:#686868; */
	text-decoration:none;
}
a:hover{
	/* color:#ff0000; */
	text-decoration:underline;
}
ul{
	list-style:none;
}
li{
	list-style:none;
}
input,button,select{
	font-size:16px;
	font-family: "Microsoft YaHei" ! important;
	/* vertical-align:middle;  */
	outline:none;cursor: pointer;
	
}
/* 
 select{appearance: none;
	-webkit-appearance: none;
	-moz-appearance: none;
	-ms-appearance: none; 
	-o-appearance: none;}
	
	
body logo bg*/
body{width:1920px;height:1080px;background:url(../images/showpageIMG/showpagebg.jpg) no-repeat;}
.companyLogo{position:absolute;left:10px;top:10px;}
/* 4 block*/
.Block{width:900px;height:450px;box-sizing:border-box;box-shadow:0 3px 5px rgba(0, 0, 0, 0.6);float:left;border-radius:4px;background:#fff;overflow:hidden}
.block1{margin-left:37px;margin-top:53px}
.block2{margin-left:44px;margin-top:53px}
.block3{margin-left:37px;margin-top:74px}
.block4{margin-left:44px;margin-top:74px}
.Block .greenBanner{width:100%;height:70px;background:#1abd9b;box-shadow:0 1px 2px rgba(0, 0, 0, 0.3);border-top-left-radius:4px;border-top-right-radius:4px;}
.Block .content{width:100%;height:380px;}
.content-left, .content-right{float:left;height:380px;overflow:hidden}
.content-left{width:199px;border-right:1px solid #696969}
.content-right{width:700px}
.content3 .content-left{border-right:1px solid #ccc}
.six-num{width:100%;height:126px;border-bottom:1px solid #696969}
.six-num p{margin-left:44px}
.six-num p:nth-child(1){font-size:26px;padding-top:25px}
.six-num p:nth-child(2){font-size:22px;padding-top:10px}
.head-1,.head-2{float:left;line-height:70px;height;70px;color:#fff}
.head-1{font-size:28px;width:140px;padding-left:60px;}
.head-2{font-size:22px}
.head-3{float:right;line-height:70px;height;70px;font-size:23px;color:#fff;width:190px}
.greenBanner1 .head-1{
background:url(../images/showpageIMG/renyuan.png) 20px 20px no-repeat;
}
.greenBanner2 .head-1{
background:url(../images/showpageIMG/yewutongji.png) 20px 20px no-repeat;
}
.greenBanner3 .head-1{
background:url(../images/showpageIMG/shebeiditu.png) 20px 20px no-repeat;
}
.greenBanner4 .head-1{
background:url(../images/showpageIMG/fuwushishi.png) 20px 20px no-repeat;
}
.head-2 select{background:#199c81;font-size:14px;height:30px;color:#fff;border:none}
.head-2 select option{color:#fff;font-size:14px}
.greenBanner1 .head-2 select:nth-child(4){width:150px}
.greenBanner1 .head-2 select{width:80px}
.greenBanner2 .head-2 select:nth-child(2){width:76px}
.greenBanner2 .head-2 select:nth-child(1){width:96px;margin-left:20px}
.head-2 input{
background:url(../images/showpageIMG/f5.png)  no-repeat;
border:0;width:30px;height:30px;padding:0 0 11px 0;margin-left:10px
}
.head-2 input:hover,.head-2 input:active{
background:url(../images/showpageIMG/f51.png)  no-repeat;
border:0;width:30px;height:30px;padding:0 0 11px 0;margin-left:10px
}
.greenBanner3 .head-2 span{padding-left:22px;color:#fff}
.greenBanner3 .head-2 span:nth-child(1){
background:url(../images/showpageIMG/ling.png) 0 5px no-repeat;margin-left:30px
}
.greenBanner3 .head-2 span:nth-child(2){
background:url(../images/showpageIMG/dian.png) 0 5px no-repeat;margin-left:20px
}
.head-3{
background:url(../images/showpageIMG/v3.png) 145px 24px no-repeat;
cursor:pointer;position:relative;
}
.content4 table{width:100%;height:370px;border-collapse:collapse;margin-top:10px !important}
.content4 table thead{background:#d7d8d8;font-size:18px;text-align:left;height:36px;line-height:36px;}
.content4 table thead tr { display:block; } 
.content4 table th{font-weight:normal}
.content4 table tbody{font-size:16px; display: block; height: 334px; overflow-y:scroll;overflow-x:hidden;line-height:38px;
}
.content4 table th:nth-child(1),.content4 table td:nth-child(1){padding-left:30px;width:210px}
.content4 table th:nth-child(2),.content4 table td:nth-child(2){width:130px}
.content4 table th:nth-child(3),.content4 table td:nth-child(3){width:130px}
.content4 table th:nth-child(4),.content4 table td:nth-child(4){width:150px}
#scroll-1::-webkit-scrollbar {
width:10px;
height:10px;
}
#scroll-1::-webkit-scrollbar-thumb{
background:#1abc9c;border-radius:4px;
}
#scroll-1::-webkit-scrollbar-track     {
background:#fff;
}
.overview ul li {height:60px;border-bottom:1px solid #d7d8d8;cursor:pointer}
.overview ul li a{display:block;width:199px;height:60px;text-decoration:none}
.overview ul li a:hover {text-decoration:none} 

.overview ul li p:nth-child(1){font-size:18px;padding-top:10px;padding-left:50px}
.overview ul li p:nth-child(2){font-size:14px;padding-left:50px}
.black-dingwei{background:url(../images/showpageIMG/black-dingwei.png) 15px 15px no-repeat,#fff;}
.black-anhu{background:url(../images/showpageIMG/black-anhu.png) 15px 15px no-repeat,#fff;}
.redpointer{background-color:#DF4B33;}
.black-dingwei:hover,.black-anhu:hover{background-color:#eee;}
/* �ֲ�ͼ*/
.my-slide>div{width:700px;height:380px;}
.navigation{position:absolute;right:40px;top:-70px;width:150px;display:none;cursor:pointer;z-index:55; }
.navigation span:nth-child(1){margin-top:70px;}
.navigation span{display:block;font-size:18px;border-bottom:1px solid #bebebe;line-height:50px;background:#fff;height:50px;color:#fff;
-webkit-box-shadow: 1px 1px 4px #000;  
  -moz-box-shadow: 1px 1px 4px #000;  
  box-shadow: 1px 1px 4px #000;}
.navigation span:hover{background:#e2e2e2;color:#e2e2e2}
.navigation span.active{background:#e2e2e2;color:#e2e2e2}



