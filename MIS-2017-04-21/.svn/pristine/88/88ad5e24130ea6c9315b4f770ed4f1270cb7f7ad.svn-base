	   /**
		 * 保存记录
		 * 
		 * @param obj
		 */
		public boolean save(DepartmentEntity department) {			
			boolean result =false;
			if(department!=null){
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					 tx.beginTransaction();					 					 
					 					 				     				     				     				     					     //关联信息保存
					     DepartmentEntity parentDepartment = department.getParentDepartment();
						 if(parentDepartment!=null){						   
							 dbManager.saveNoTransaction(parentDepartment);
							 department.setParentId(parentDepartment.getId());
						 }	
						 //关联信息保存
						 if(department.getChildDepartmentList()!=null&&department.getChildDepartmentList().size()>0){						   			    	            	    	           
							    for(DepartmentEntity departmentEntity:department.getChildDepartmentList()){
							    	dbManager.saveNoTransaction(departmentEntity);
							    }
						 }					 					
				     					 					 				     				     					     //关联信息保存
					     UserEntity createUser = department.getCreateUser();
						 if(createUser!=null){						   
							 dbManager.saveNoTransaction(createUser);
							 department.setCreateUserId(createUser.getId());
						 }						 
				     				     				     					 					 				     				     					     //关联信息保存
					     UserEntity updateUser = department.getUpdateUser();
						 if(updateUser!=null){						   
							 dbManager.saveNoTransaction(updateUser);
							 department.setUpdateUserId(updateUser.getId());
						 }						 
				     				     				     					 					 result=dbManager.saveNoTransaction(department);	
					 tx.commitAndClose();  
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {				    
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  			
			}	
			return result;
		}
		
		/**
		 * 批量保存记录
		 * 
		 * @param list
		 */
		public boolean saveList(List<DepartmentEntity> departmentList) {
			boolean result = false;
			if (departmentList != null && departmentList.size() > 0) {
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					tx.beginTransaction(); 
					for(DepartmentEntity department:departmentList){
						if(department!=null){												  								 
								 									 					                 								     								     								     									     //关联信息保存
									     DepartmentEntity parentDepartment = department.getParentDepartment();
										 if(parentDepartment!=null){						   
											 dbManager.saveNoTransaction(parentDepartment);
											 department.setParentId(parentDepartment.getId());
										 }	
										  //关联信息保存	
										 if(department.getChildDepartmentList()!=null&&department.getChildDepartmentList().size()>0){						   						    	               	           
										    for(DepartmentEntity departmentEntity:department.getChildDepartmentList()){
										    	dbManager.saveNoTransaction(departmentEntity);
										    }
									     }									 
								     								 									 					                 								     								         //关联信息保存
									     UserEntity createUser = department.getCreateUser();
										 if(createUser!=null){						   
											 dbManager.saveNoTransaction(createUser);
											 department.setCreateUserId(createUser.getId());
										 }									 
								     								     								     								 									 					                 								     								         //关联信息保存
									     UserEntity updateUser = department.getUpdateUser();
										 if(updateUser!=null){						   
											 dbManager.saveNoTransaction(updateUser);
											 department.setUpdateUserId(updateUser.getId());
										 }									 
								     								     								     								 								 result=dbManager.saveNoTransaction(department);								 			
						}	
					}
				    tx.commitAndClose(); 
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  
			}
			return result;
		}
		
