		/**
		 * 删除记录
		 * 
		 * @param id 主键
		 * @param obj
		 */
		public boolean del(Integer id,Boolean delParentDepartment,Boolean delChildDepartmentList,Boolean delCreateUser,Boolean delUpdateUser) {
			boolean result = false;
			if (id !=null&&id>0) {
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					tx.beginTransaction(); 					
										 	                 				     				     				     					    //删除关联信息
						if(delParentDepartment!=null&&delParentDepartment.booleanValue()){	
						    DepartmentEntity department = (DepartmentEntity)dbManager.getById(id, DepartmentEntity.class);
						    if(department.getParentId()!=null){
							dbManager.delNoTransaction(department.getParentId(), DepartmentEntity.class);
						    }						
					    }
					    //删除关联信息
						if(delChildDepartmentList!=null&&delChildDepartmentList.booleanValue()){
							QueryCondition qc = new QueryCondition(DepartmentEntity.PARENT_ID, QueryCondition.eq, id);
							dbManager.delByConditionsNoTransaction(DepartmentEntity.class, qc);
						}
				     										 	                 				     					    //删除关联信息
						if(delCreateUser!=null&&delCreateUser.booleanValue()){	
							DepartmentEntity department = (DepartmentEntity)dbManager.getById(id, DepartmentEntity.class);
							if(department!=null&&department.getCreateUserId()!=null){
								dbManager.delNoTransaction(department.getCreateUserId(), UserEntity.class);
							}						
						}
				     				     				     										 	                 				     					    //删除关联信息
						if(delUpdateUser!=null&&delUpdateUser.booleanValue()){	
							DepartmentEntity department = (DepartmentEntity)dbManager.getById(id, DepartmentEntity.class);
							if(department!=null&&department.getUpdateUserId()!=null){
								dbManager.delNoTransaction(department.getUpdateUserId(), UserEntity.class);
							}						
						}
				     				     				     										result = dbManager.delNoTransaction(id, DepartmentEntity.class);
					tx.commitAndClose(); 
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  
			}
			return result;
		}
		
		/**
		 * 批量删除记录
		 * 
		 * @param ids
		 *            主键 英文逗号间隔
		 * @param obj
		 */
		public boolean del(String ids) {
			boolean result = false;
			if (ids != null && !"".equals(ids.trim())) {
				TransactionManager tx = DbUtils.getTranManager();
				try {
					tx.beginTransaction();
					QueryCondition qc = new QueryCondition(DepartmentEntity.ID,QueryCondition.in, ids);
					result = dbManager.delByConditionsNoTransaction(DepartmentEntity.class, qc);
					tx.commitAndClose();
				} catch (Exception e) {
					logger.error("数据库提交失败！");
					logger.error(e);
					result = false;
					try {
						tx.rollbackAndClose();
					} catch (Exception ex) {
						logger.error("数据库回滚失败！");
						logger.error(ex);
					}
				}
			}
			return result;
		}

