		/**
		 * 根据id读取记录
		 * @param id 主键
		 		             	                 				     				     				     		 * @param parentDepartmentShow 是否查询关联信息
		   @param childDepartmentListShow 是否查询关联信息
				     		 		             	                 				     		 * @param createUserShow 是否查询关联信息
				     				     				     		 		             	                 				     		 * @param updateUserShow 是否查询关联信息
				     				     				     		 		 * @param obj
		 */
		public DepartmentEntity getById(Integer id,Boolean parentDepartmentShow,Boolean childDepartmentListShow,Boolean createUserShow,Boolean updateUserShow) {
			DepartmentEntity obj = null;
			if (id!=null) {
				obj = (DepartmentEntity)dbManager.getById(id, DepartmentEntity.class);
									 	                 				     				     				     				        //查询关联内容
						if(parentDepartmentShow!=null&&parentDepartmentShow.booleanValue()&&obj!=null&&obj.getParentId()>0){
					        DepartmentEntity department = (DepartmentEntity)dbManager.getById(obj.getParentId(), DepartmentEntity.class);
					        obj.setParentDepartment(department);        	  	  
				        }
				        //查询关联内容
						if(childDepartmentListShow!=null&&childDepartmentListShow.booleanValue()&&obj!=null&&obj.getId()>0){
							List<Object> list = dbManager.searchListByColumn(DepartmentEntity.class, DepartmentEntity.PARENT_ID, String.valueOf(id));
			            	  if(list!=null&&list.size()>0){
			            		  List<DepartmentEntity> childDepartmentList= new ArrayList<DepartmentEntity>();
			            		  for(Object o:list){
			            			  childDepartmentList.add((DepartmentEntity)o);
			            		  }
			            		  obj.setChildDepartmentList(childDepartmentList);          		  
			            	  }	            	  	  
						}
				     				   					 	                 				     				        //查询关联内容
						if(createUserShow!=null&&createUserShow.booleanValue()&&obj!=null&&obj.getCreateUserId()>0){
							UserEntity createUser = (UserEntity)dbManager.getById(obj.getCreateUserId(), UserEntity.class);
							obj.setCreateUser(createUser);        	  	  
						}
				     				     				     				   					 	                 				     				        //查询关联内容
						if(updateUserShow!=null&&updateUserShow.booleanValue()&&obj!=null&&obj.getUpdateUserId()>0){
							UserEntity updateUser = (UserEntity)dbManager.getById(obj.getUpdateUserId(), UserEntity.class);
							obj.setUpdateUser(updateUser);        	  	  
						}
				     				     				     				   			}
			return obj;
		}
		
