	<script type="text/javascript">
//根据排序规则重新查询
function queryBySort(par,column){
	var classFlag = listSortClickEvent(par);
	var type = judgmentChinese(column);
	if(type != null && type != ""){
		classFlag = type + classFlag;
		column = column.substr(8,column.length);
	}
	$("#sortType").val(classFlag);
	$("#sortColumn").val(column);
	go2page(currentshownpage);
}

	  var currentshownpage = 1;
	  var path = "site.do";

	  //获取市信息
      function getCityInformation(id){
        var result = new Array();
    	result = getAddresses(id);
        var html = "";
        html += "<option value=\"市\">市</option>";
        for(var i = 0; i < result.length; i++){
        	html += "<option value=\"" + result[i].id + "\">" + result[i].name + "</option>";
        }
        $("#quyushi_input").html(html);
      }
      
      //获取区信息
      function getAreaInformation(id){
        var result = new Array();
    	result = getAddresses(id);
        var html = "";
        html += "<option value=\"区\">区</option>";
        for(var i = 0; i < result.length; i++){
        	html += "<option value=\"" + result[i].id + "\">" + result[i].name + "</option>";
        }
        $("#quyuqu_input").html(html);
      }
      
      //获取街道信息
      function getStreetInformation(id){
        var result = new Array();
    	result = getAddresses(id);
        var html = "";
        html += "<option value=\"街道\">街道</option>";
        for(var i = 0; i < result.length; i++){
        	html += "<option value=\"" + result[i].id + "\">" + result[i].name + "</option>";
        }
        $("#quyujiedao_input").html(html);
      }

	  //获取当前登录人所在站点
	  function getLoginSite(userId_login){
		  if(userId_login == null || userId_login == ""){
		  	  $("#userSiteId_login").val("");
		  	  return;
		  }
		  var reqmsg = "{'action':'QUERY_EMPLOYEE_LIST_REQUEST','page':{'pageno':'1','pagesize':'1'},'content':{'siteListShow':'true','userId':"+userId_login+"}}";
		  var siteIds = "";
		  
		  jQuery.ajax({
          	type : "post",
         	async:false,
          	url : "employee.do?handler",
          	dataType : "json",
          	data: {
               	"reqmsg":reqmsg
          	},
         	success : function(data){
              if(data.des == "success"){
            	  if (data.content != null) {
            	  	if(data.content.employeeList != null && data.content.employeeList.length == 1){
            	  	   jQuery.each(data.content.employeeList[0].siteList, function(i, item) {
            	  	   		siteIds += item.id + ",";
            	  	   		siteIds.replace(",,",",");
            	  	   });
            	  	   if(siteIds != ""){
							siteIds = siteIds.substring(0, siteIds.length-1);
					   }
					   $("#userSiteId_login").val(siteIds);
            	  	}
            	  }
              }else if(data.des == "failure"){
                 alert("查询失败");
              }
          	},
          	error:function(){
	           alert("error");
          	}
     	  });
	  }
	
	  $(document).ready(function(){
			var userId_login = "<%=request.getAttribute("userId")%>";
			getLoginSite(userId_login);
	
    		App.init();
    		$(".md-trigger").modalEffects();
	
				//初始化设置地址搜索条件选择框
				//获取省  
	        var result = new Array();
	        result = getAddresses(4897);
	        var html = "";
	        html += "<option value=\"省\">省</option>";
	        for(var i = 0; i < result.length; i++){
	        	html +="<option value=\"" + result[i].id + "\">" + result[i].name + "</option>";
	        }
	        $("#quyushen_input").html(html);
	        $("#quyushi_input").html("<option value=\"市\">市</option>");
	        $("#quyuqu_input").html("<option value=\"区\">区</option>");
	        $("#quyujiedao_input").html("<option value=\"街道\">街道</option>");
	        //省级选择框变化事件
	        $("#quyushen_input").change(function(e){
	        	if($("#quyushen_input").val() == "省"){
	        		$("#quyushi_input").html("<option value=\"市\">市</option>");
	        		$("#quyuqu_input").html("<option value=\"区\">区</option>");
	        		$("#quyujiedao_input").html("<option value=\"街道\">街道</option>");
	        	}else{
	        		getCityInformation($("#quyushen_input").val());
	        		getAreaInformation($("#quyushi_input").val());
	        		getStreetInformation($("#quyuqu_input").val());
	        	}
	        });
	        //市级选择框变化事件
	        $("#quyushi_input").change(function(e){
	        	if($("#quyushi_input").val() == "市"){
	        		$("#quyuqu_input").html("<option value=\"区\">区</option>");
	        		$("#quyujiedao_input").html("<option value=\"街道\">街道</option>");
	        	}else{
	        		getAreaInformation($("#quyushi_input").val());
	        		getStreetInformation($("#quyuqu_input").val());
	        	}
	        });
	        //区级选择框变化事件
	        $("#quyuqu_input").change(function(e){
	        	if($("#quyuqu_input").val() == "区"){
	        		$("#quyujiedao_input").html("<option value=\"街道\">街道</option>");
	        	}else{
	        		getStreetInformation($("#quyuqu_input").val());
	        	}
	        });
	        
	        querySiteList();
	  });

	  function selectAll() {
		  if ($("#allselectchecker").is(':checked')) {
			  $(".col_selector").prop("checked", true);
		  } else {
			  $(".col_selector").prop("checked", false);
		  }
	  }

