	  }

	//批量删除站点
	function batchDeleteSite() {
		var selectedItemNumber = $("tbody .col_selector:checked").length;
		if (selectedItemNumber >= 1) {
			var ids2del = "";
			$("tbody .col_selector:checked").each(function() {
				ids2del += $(this).val() + ",";
			});
			if (ids2del != "") {
				ids2del = ids2del.substring(0, ids2del.length - 1);
			}
			if (confirm("是否确认删除?"))  { 
			jQuery.ajax({
				type:"post",
				url:path + "?del",
				async:true,
				dataType:"json",
				data:{ids:ids2del},
				success:function(data) {
					alert(data.des);
					if (data.result == "success") {
						go2page(currentshownpage);
					}else if(data.des == "failure"){
	                 alert("删除失败");
	                }
				},
				error:function() {
					alert("error");
				}
			});
			}
		} else {
			alert("请选择要删除的数据");
		}
	}

	//单个删除站点
	function singleDeletionSite(id) {
		  if (confirm("是否确认删除?"))  { 
			jQuery.ajax({
				type:"post",
				url:path + "?del",
				async:true,
				dataType:"json",
				data:{ids:id},
				success:function(data) {
					alert(data.des);
					if (data.result == "success") {
						go2page(currentshownpage);
					}else if(data.des == "failure"){
	                 alert("删除失败");
	                }
				},
				error:function() {
					alert("error");
				}
			});
		  }
	}

	//根据站长姓名查找他的ID，搜索条件用
	function getSiteNameById(name){
		var id = "";
		jQuery.ajax({
	          type : "post",
	          async:false,
	          url : path + "?getId",
	          dataType : "json",
	          data: {
	               "name":name
	          },
	          success : function(data){
	             id = data.ID;
	          },
	          error:function(){
		           alert("error");
	          }
	     });
	     return id;
	}

	//根据ID查找姓名
