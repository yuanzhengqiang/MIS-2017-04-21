	   /**
		 * 保存记录
		 * 
		 * @param obj
		 */
		public boolean save(ServicePhotoEntity servicePhoto) {			
			boolean result =false;
			if(servicePhoto!=null){
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					 tx.beginTransaction();					 					 
					 					 				     				     					     //关联信息保存
					     ServiceEntity service = servicePhoto.getService();
						 if(service!=null){						   
							 dbManager.saveNoTransaction(service);
							 servicePhoto.setServiceId(service.getId());
						 }						 
				     				     				     					 					 result=dbManager.saveNoTransaction(servicePhoto);	
					 tx.commitAndClose();  
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {				    
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  			
			}	
			return result;
		}
		
		/**
		 * 批量保存记录
		 * 
		 * @param list
		 */
		public boolean saveList(List<ServicePhotoEntity> servicePhotoList) {
			boolean result = false;
			if (servicePhotoList != null && servicePhotoList.size() > 0) {
				TransactionManager tx = DbUtils.getTranManager();   
				try{   
					tx.beginTransaction(); 
					for(ServicePhotoEntity servicePhoto:servicePhotoList){
						if(servicePhoto!=null){												  								 
								 									 					                 								     								         //关联信息保存
									     ServiceEntity service = servicePhoto.getService();
										 if(service!=null){						   
											 dbManager.saveNoTransaction(service);
											 servicePhoto.setServiceId(service.getId());
										 }									 
								     								     								     								 								 result=dbManager.saveNoTransaction(servicePhoto);								 			
						}	
					}
				    tx.commitAndClose(); 
				}catch (Exception e) { 
					 logger.error("数据库提交失败！");
					 logger.error(e);
					 result = false;
				     try {
						   tx.rollbackAndClose();
					 } catch (Exception ex) {					   
						   logger.error("数据库回滚失败！");
						   logger.error(ex);
					 }   
				}  
			}
			return result;
		}
		
