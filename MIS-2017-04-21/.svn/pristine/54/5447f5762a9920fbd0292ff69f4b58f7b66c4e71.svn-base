		//测量时间转化 YYYYMMDDhhmmss
		function changeTime(time){
			time = time.split("-")[0] + time.split("-")[1] + time.split("-")[2];
			time = time.split(":")[0] + time.split(":")[1];
			time = time.replace(/\ +/g,"");
			return time + "00";
		}
