		function del_history(id){
			 if (confirm("是否确认删除?"))  { 
				jQuery.ajax({
					type:"post",
					url:"dailyLife.do?del",
					async:false,
					dataType:"json",
					data:{ids:id},
					success:function(data) {
						if (data.result == "success") {
							updateOlder_shls();
							rcshQuery();
							go2page_history(currentshownpage_history);
							alert(data.des);
						}else if(data.des=="failure"){
		                 	alert("删除失败");
		                }
					},
					error:function() {
						alert("error");
					}
				});
			  }
		}
		function del_historyAll(){
			var selectedItemNumber = $("tbody .col_selector_history:checked").length;
			if (selectedItemNumber >= 1) {
				var ids2del = "";
				$("tbody .col_selector_history:checked").each(function() {
					ids2del += $(this).val() + ",";
				});
				if (ids2del != "") {
					ids2del = ids2del.substring(0, ids2del.length - 1);
				}
				
			 if (confirm("是否确认删除?"))  { 
				jQuery.ajax({
					type:"post",
					url:"dailyLife.do?del",
					async:false,
					dataType:"json",
					data:{ids:ids2del},
					success:function(data) {
						if (data.result == "success") {
							updateOlder_shls();
							rcshQuery();
							go2page_history(currentshownpage_history);
							alert(data.des);
						}else if(data.des=="failure"){
		                 	alert("删除失败");
		                }
					},
					error:function() {
						alert("error");
					}
				});
			  }
			} else {
				alert("请选择要删除的数据");
			}
		}
		//转化日期2
	    function riqiChange2(data){
	   		var date_=data;
	    	var date2=data.substr(0,4)+"-"+data.substr(4,2)+"-"+data.substr(6,2)+" "+data.substr(8,2)+":"+data.substr(10,2);
	    	if(date2 == "--"){
	    		date2 = "无";
	    	}
	    	return date2;
	    }
		function ckxq_history(id){
			var reqmsg="{'action':'QUERY_DAILY_LIFE_INFO_REQUEST','content':{";
			reqmsg += "\"id\":" + id + "}}";
		
			jQuery.ajax({
				type:"post",
				url:"dailyLife.do?handler",
				async:true,
				dataType:"json",
				data: {
	               "reqmsg":reqmsg
	          	},
				success:function(data) {
					if (data.des == "success") {
						if(data.content != null){
							$("#rcshjl_id_hide").val(data.content.id);
							$("#rcshjl_time").val(riqiChange2(data.content.createTime));
							$("#rcshjl_pdr").val(data.content.createPerson);
	    					$("#rcshjl_rshl").val(data.content.dailyWater);
	    					$("#rcshjl_rsyll").val(data.content.dailyDrinking);
	    					$("#rcshjl_rshlzs").val(data.content.mainFood);
	    					$("#rcshjl_rshlyou").val(data.content.oil);
	    					$("#rcshjl_rshlyan").val(data.content.salt);
	    					$("#rcshjl_rshlyu").val(data.content.fish);
	    					$("#rcshjl_rshlgs").val(data.content.fruitsVegetables);
	    					$("#rcshjl_rshlnai").val(data.content.milk);
	    					$("#rcshjl_rshlrou").val(data.content.meat);
	    					$("#rcshjl_rshldzp").val(data.content.beanProducts);
	    					$("#rcshjl_xbcs").val(data.content.urine);
	    					$("#rcshjl_xbl").val(data.content.urineNum);
	    					$("#rcshjl_dbcs").val(data.content.shit);
	   						$("#rcshjl_dbts").val(data.content.shitDay);
	   						$("#rcshjl_smsj").val(data.content.sleepHour);
	    					$("#rcshjl_smzj").val(data.content.sleepQuality);
	    					$("#rcshjl_jinshen").val(data.content.morningSpirit);
						}
					}else{
	                 	alert("查询失败");
	                }
				},
				error:function() {
					alert("error");
				}
			});
		
			$("#rcshlsjl_bj").click();
		}
		//更新Old生活历史 
		function updateOlder_shls(){
		   var olderId="<%=request.getAttribute("olderId")%>";
		   var result = "1";
		   jQuery.ajax({
				type:"post",
				url:"dailyLife.do?getOlderId",
				async:false,
				dataType:"json",
				data:{
					"olderId":olderId
				},
				success:function(data) {
					if (data.result == "success") {
						result = "1";
					}else if(data.des=="failure"){
						result = "0";
	                 	alert("更新Old生活历史失败");
	                }
				},
				error:function() {
					result = "0";
					alert("error_upold");
				}
		   });
		}
		
