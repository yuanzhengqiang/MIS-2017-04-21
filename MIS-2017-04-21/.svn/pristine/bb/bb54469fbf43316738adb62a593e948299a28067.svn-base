@TableDescription(name = "B_MED_NOTICE")
public class MedNoticeEntity implements java.io.Serializable {
    public final static String ID = "ID";
    public final static String NOTICE_TIME = "NOTICE_TIME";
    public final static String NOTICE_CONTENT = "NOTICE_CONTENT";
    public final static String MED_PLAN_ID = "MED_PLAN_ID";
    
    /**
	 * 主键
	 */
	@ColumnDescription(name = "ID", isPrimaryKey = true, isAutoIncrement = true)
	private Integer id;
    /**
	 * 提醒时间
	 */
	@ColumnDescription(name = "NOTICE_TIME")
	private String noticeTime;
    /**
	 * 提醒内容
	 */
	@ColumnDescription(name = "NOTICE_CONTENT")
	private String noticeContent;
    /**
	 * 服药计划ID
	 */
	@ColumnDescription(name = "MED_PLAN_ID")
	private Integer medPlanId;
        
    public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}
    public String getNoticeTime() {
		return noticeTime;
	}

	public void setNoticeTime(String noticeTime) {
		this.noticeTime = noticeTime;
	}
    public String getNoticeContent() {
		return noticeContent;
	}

	public void setNoticeContent(String noticeContent) {
		this.noticeContent = noticeContent;
	}
    public Integer getMedPlanId() {
		return medPlanId;
	}

	public void setMedPlanId(Integer medPlanId) {
		this.medPlanId = medPlanId;
	}
        
    /**
     * 关系描述
	 */
	    @RelationlDescription(relation = "ManyToOne",
		joinEntity="MedPlanEntity"	,joinColumn="ID"	)
			
			
		private MedPlanEntity medPlan;
	        
	public MedPlanEntity getMedPlan() {
		return medPlan;
    }
	
	public void setMedPlan(MedPlanEntity medPlan) {
		this.medPlan = medPlan;
	}
			
}
