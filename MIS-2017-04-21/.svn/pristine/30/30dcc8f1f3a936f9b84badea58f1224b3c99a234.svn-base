		//查询老人概览信息
		function query(){
			var id = "<%=request.getAttribute("olderId")%>";
			var reqmsg = "{'action':'QUERY_OLDER_INFO_REQUEST','content':{\"olderAddressListShow\":\"true\",\"id\":" + id + "}}";
			jQuery.ajax({
	          type : "post",
	          async:false,
	          url : "daeOlder.do?handler",
	          dataType : "json",
	          data: {
	               "reqmsg":reqmsg
	          },
	          success : function(data){
	              if(data.des == "success"){
	            	  changeData(data);
	              }else{
	                  alert("查询老人概览信息失败");
	              }
	          },
	          error:function(){
		          alert("查询老人概览信息失败");
	          }
	    	});
		}
	
