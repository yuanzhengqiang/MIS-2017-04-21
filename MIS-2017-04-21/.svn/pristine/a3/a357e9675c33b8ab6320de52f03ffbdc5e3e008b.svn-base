		 * @param {Number} totalRecord 总记录数
		 */
		function genarateRecordNumberHtml2(currentpage, pagesize, totalRecord) {
			var recordInfos = "";
			if (totalRecord > 0) {
			    recordInfos = "当前显示" + totalRecord + "条总记录";
				recordInfos += "中第" + ((currentpage - 1) * pagesize + 1) + "-";
